<Event>
<Name>On Preload</Name>
<Args/>
<Script>--Assign Variables
CancelActions = false;
VehicleHAN_Lines = {};
VehicleIDE_Lines = {};
VehicleCOL_Lines = {};
UseOriginalHAN = false;
UseOriginalIDE = false;
UseOriginalCOL = false;
car4=3




--Get vehicle short name only (without the .wft extension)
VehicleShortName_Table = String.SplitPath(wftPath);
VehicleShortName = VehicleShortName_Table.Filename;


if VehicleShortName=="voodoo" 
or VehicleShortName=="futo" 
or VehicleShortName=="huntley" 
or VehicleShortName=="primo"
or VehicleShortName=="ruiner"
or VehicleShortName=="sultan"
or VehicleShortName=="sultanrs"
or VehicleShortName=="zombieb" then
car4=4
end



--&lt;-Get All Necessary Original Info From Original IDE, Handling, Carcols Lines-&gt;--


--Get original IDE line for vehicle--------------------------------------------------------------
VehicleIDELinePosReturn, VehicleIDELineReturn = SearchTable(VehicleShortName,VehicleList);
VehicleIDELinePos = VehicleIDELinePosReturn[1];
VehicleIDELine = VehicleData[VehicleIDELinePos];
--Assign Original Line to String Variable
VehicleIDE_Original = VehicleIDELine;
--Remove Tabs and Doublespaces
VehicleIDE_Original_Cleaned = RemoveTabDoubleSpace(VehicleIDE_Original);
--Delimi IDE Line to A Table
VehicleIDE_Original_Delim = Delimit(VehicleIDE_Original_Cleaned, ",", false)
if (VehicleIDE_Original_Delim == nil) then
	CancelActions = true;
change_lang_message("page_Vehicle_Info_texts","33","none","ok")
	
	Error260 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
	--Log Error
	ErrorLog_Value = "ERROR 260 - "..text_message.."";
	ErrorLogUpdate();
	ExitGIMI();
end
--------------------------------------------------------------------------------------------------



--Break IDE Line Into a Table With Each Value
VehicleIDE_Table = Delimit(VehicleIDELine, ",", true);

--Get Vehicle Data File Values
VehicleIDE_Handling = VehicleIDE_Table[5];
VehicleIDE_Model = VehicleIDE_Table[2];
VehicleIDE_Number = VehicleIDE_Table[1];
VehicleIDE_Type = VehicleIDE_Table[4];
--Count # of Fields In IDE Line
VehicleIDE_FieldCount = Table.Count(VehicleIDE_Table);


--Get Original Handling Line----------------------------------------------------------------------
VehicleHAN_Original_SearchPos, VehicleHAN_Original_Search = SearchTable(VehicleIDE_Handling,VehicleDataH);
if (VehicleHAN_Original_Search ~= nil) then
	--Assign Original Line to String Variable
	VehicleHAN_Original = VehicleHAN_Original_Search[1];
	--Remove Tabs and Doublespaces
	VehicleHAN_Original_Cleaned = RemoveTabDoubleSpace(VehicleHAN_Original);
	--Delimi Handling Line to A Table
	VehicleHAN_Original_Delim = Delimit(VehicleHAN_Original_Cleaned, " ", false)
	if (VehicleHAN_Original_Delim == nil) then
		CancelActions = true;
change_lang_message("page_Vehicle_Info_texts","34","none","ok")
		Error261 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
		--Log Error
		ErrorLog_Value = "ERROR 261 - "..text_message.."";
		ErrorLogUpdate();
		ExitGIMI();
	end
else
	CancelActions = true;
change_lang_message("page_Vehicle_Info_texts","34","none","ok")
		Error261 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
		--Log Error
		ErrorLog_Value = "ERROR 261 - "..text_message.."";
		ErrorLogUpdate();
	ExitGIMI();
end
---------------------------------------------------------------------------------------------------

--Get Original Carcols Line------------------------------------------------------------------------
VehicleCOL_Original_HasColLineCheck = NotInCarcols(VehicleIDE_Model);
if (VehicleCOL_Original_HasColLineCheck == false) then
	VehicleCOL_Original_SearchPos, VehicleCOL_Original_Search = SearchTable(VehicleIDE_Model,VehicleDataC);
	if (VehicleCOL_Original_Search ~= nil) then
		--Assign Original Line to String Variable
		VehicleCOL_Original = VehicleCOL_Original_Search[1];
		--Remove Tabs and Doublespaces
		VehicleCOL_Original_Cleaned = RemoveTabDoubleSpace(VehicleCOL_Original);
		--Delimi Handling Line to A Table
		VehicleCOL_Original_Delim = Delimit(VehicleCOL_Original_Cleaned, " ", true)
		if (VehicleCOL_Original_Delim == nil) then
			CancelActions = true;
change_lang_message("page_Vehicle_Info_texts","35","none","ok")
			Error262 = Dialog.Message(text_title,text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
			--Log Error
			ErrorLog_Value = "ERROR 262 - "..text_message.."";
			ErrorLogUpdate();
			ExitGIMI();
		end
	else
		CancelActions = true;
change_lang_message("page_Vehicle_Info_texts","35","none","ok")
		Error262 = Dialog.Message(text_title,text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
		--Log Error
		ErrorLog_Value = "ERROR 262 - "..text_message.."";		ErrorLogUpdate();
		ExitGIMI();
	end
end
---------------------------------------------------------------------------------------------------

--Find All TXT Files Included With the Mod
Vehicle_AllTxtFiles = File.Find(ModFolder, "*.txt", true, false, nil);
if (Vehicle_AllTxtFiles ~= nil) then
	--At least one txt file was found...
	
	--------------------------------------------------------------------------------------------------------------------
	---Search All TXT Files For Custom HANDLING Line--------------------------------------------------------------------
	--------------------------------------------------------------------------------------------------------------------
	--Run SearchTxt Function
	VehicleHAN_FoundPos, VehicleHAN_FoundLines, VehicleHAN_FoundFiles = SearchTxt(VehicleIDE_Handling,Vehicle_AllTxtFiles);
	--Run Function to Verify If It Is A Valid Handling Line
	VehicleHAN_Lines = VerifyLineHAN(VehicleHAN_FoundLines,VehicleIDE_Handling);
	
	--------------------------------------------------------------------------------------------------------------------
	---Search All TXT Files For Custom IDE Line--------------------------------------------------------------------
	--------------------------------------------------------------------------------------------------------------------
	--Run SearchTxt Function
	VehicleIDE_FoundPos, VehicleIDE_FoundLines, VehicleIDE_FoundFiles = SearchTxt(VehicleIDE_Number,Vehicle_AllTxtFiles);
	--Run Function to Verify If It Is A Valid IDE Line
	VehicleIDE_Lines = VerifyLineIDE(VehicleIDE_FoundLines,VehicleIDE_Number);
	
	--------------------------------------------------------------------------------------------------------------------
	---Search All TXT Files For Custom CARCOLS Line--------------------------------------------------------------------
	--------------------------------------------------------------------------------------------------------------------
	
		--Run SearchTxt Function
		VehicleCOL_FoundPos, VehicleCOL_FoundLines, VehicleCOL_FoundFiles = SearchTxt(VehicleIDE_Model,Vehicle_AllTxtFiles);
		--Run Function to Verify If It Is A Valid Carcols Line
		VehicleCOL_Lines = VerifyLineCOL(VehicleCOL_FoundLines,VehicleIDE_Model);
end</Script>
