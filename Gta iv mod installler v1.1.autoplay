function DetectGameDir ()

	--Assign Variables
	DetectGameDir_Result = false;
	
	--Get GTA IV game dir from registry
	DetectedDir = Registry.GetValue(HKEY_LOCAL_MACHINE, "Software\\Rockstar Games\\GTA GTA IV\\Installation", "ExePath", true);
	--If game path is NOT detected, assign default game path
	if (DetectedDir == "") then
		DetectedDirFinal = DefaultGamePath;
change_lang_message("Global_functions_texts","218","none","ok")
		Error884564 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
		DetectGameDir_Result = false;
	else
		--If game path IS detected, get DRIVE and FOLDER--
		--Remove the prentheses from detected game dir
		DetectedDir_Length = String.Length(DetectedDir);
		DetectedDir_ChopLeft = DetectedDir_Length - 2;
		DetectedDir_Trimmed = String.Mid(DetectedDir, 2, DetectedDir_ChopLeft);
		--Find out the DRIVE and FOLDER path
		DetectedDir_Table = String.SplitPath(DetectedDir_Trimmed);
		DetectedDir_Drive = DetectedDir_Table.Drive;
		DetectedDir_Folder = DetectedDir_Table.Folder;
		--Join the DRIVE and FOLDER together to get final dir
		DetectedDirCompleted = String.Concat(DetectedDir_Drive, DetectedDir_Folder);
		--Make sure detected directory exists
		DetectedDirCompletedExists = File.DoesExist(DetectedDirCompleted .. "\\gtaiv.exe");
		if (DetectedDirCompletedExists ~= false) then --If detected directory DOES exist...
			DetectedDirFinal = DetectedDirCompleted;
			DetectGameDir_Result = true;
			--Assign LastUsedPath in gimi.ini file
			INIFile.SetValue(gimiIniFile, "gimi", "LastUsedGamePath", DetectedDirFinal);
		else --If detected directory does NOT exist...
			--Assign default game path
			DetectedDirFinal = DefaultGamePath;
change_lang_message("Global_functions_texts","218","none","ok")
		Error884564 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
		end
	end
end

   
