		Scripter_EditType = "modify";
		Scripter_Modify_CommandPosition = Scripter_Modify_CurrentSelected;
		Scripter_Edit_CurrentCmd = Scripter_Modify_CurrentText;
		Scripter_RunEditing = true;
		Page.Jump("Scripter_Input");
	end
end

function Scripter_Save ()
	CancelActions = false;
	
	--Assign Variables
	Scripter_ListCount = 0;
	Scripter_Save_Counter = 0;
	Scripter_Save_CurrentCommand = "";
	Scripter_Save_Table = {};
	
	--Count the number of commands in list
	Scripter_ListCount = ListBox.GetCount("commandlist");
	if (Scripter_ListCount == 0) then
		CancelActions = true;
change_lang_message("page_Scripter{1}_texts","95","none","ok")
		result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
	else
		while (Scripter_Save_Counter &lt; Scripter_ListCount) do
			Scripter_Save_Counter = Scripter_Save_Counter + 1;
			--Get next command in list
			Scripter_Save_CurrentCommand = ListBox.GetItemText("commandlist", Scripter_Save_Counter);
			if (Scripter_Save_CurrentCommand == "") then
				CancelActions = true;
change_lang_message("page_Scripter{1}_texts","96","none","ok")
		result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
			else
				Table.Insert(Scripter_Save_Table, Scripter_Save_Counter, Scripter_Save_CurrentCommand);
			end
		end
		--Count table to make sure there are commands
		Scripter_Save_Table_Count = Table.Count(Scripter_Save_Table);
		if (Scripter_Save_Table_Count == 0) then
change_lang_message("page_Scripter{1}_texts","97","none","ok")
		result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
		else
			TextFile.WriteFromTable(Scripter_Script, Scripter_Save_Table, false);
			Scripter_Save_SuccessCheck = Application.GetLastError();
			if (Scripter_Save_SuccessCheck ~= 0) then
				CancelActions = true;
change_lang_message("page_Scripter{1}_texts","98","none","ok")
		result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
					else
				Scripter_SavePrompt = false;
			end
		end
	end
end



function Scripter_Load () --define main script function
	CancelActions = false;

	--Assign Misc. Variables
	SCRIPT_CurrentScriptLine = 0;
	--Reset Misc. Command Variables
	ScriptActions_TableNum = 0;
	--Create Tables
	ScriptActions_1 = {};
	ScriptActions_2 = {};
	ScriptActions_3 = {};
	ScriptActions_4 = {};
	ScriptActions_5 = {};

	--Assign Final Script Path Variable
	ActiveScript = Scripter_Script;
	--Read script to a table &amp; count script lines\commands
	--Get info for FIRST script
	SCRIPT_ScriptTable = TextFile.ReadToTable(ActiveScript);
	if (SCRIPT_ScriptTable == nil) then
		CancelActions = true;
change_lang_message("page_Scripter{1}_texts","99","none","ok")
		Error101s = Dialog.Message(text_title, text_message, MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);	
	else --If script DOES exist...
		--Count number of lines in the script
		SCRIPT_ScriptCommandCount = Table.Count(SCRIPT_ScriptTable);
		if (SCRIPT_ScriptCommandCount == 0) then
			CancelActions = true;
change_lang_message("page_Scripter{1}_texts","100","none","ok")
			Error102s = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);		
		else
			--Get Next Command
			while ((SCRIPT_CurrentScriptLine &lt; SCRIPT_ScriptCommandCount) and (CancelActions ~= true)) do
				--Empty Variables
				SCRIPT_CurrentScriptCommand = "";
				SCRIPT_CurrentCommandTableColumn = 0;
				--Move to next line in script
				SCRIPT_CurrentScriptLine = SCRIPT_CurrentScriptLine + 1;
				--Get Next Command
				SCRIPT_CurrentScriptCommand = SCRIPT_ScriptTable[SCRIPT_CurrentScriptLine];
				--Assign Variables
				SCRIPT_CurrentSEPPos = 0;
				SCRIPT_CurrentCmdAction = nil;
				--Count Number of Characters in the CurrentScriptCommand
				SCRIPT_CurrentScriptLength = String.Length(SCRIPT_CurrentScriptCommand);
				if (SCRIPT_CurrentScriptLength == -1) then
					CancelActions = true;
change_lang_message("page_Scripter{1}_texts","101","none","ok")
					Error103s = Dialog.Message(text_title, text_message, MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);					
				else
					--Create Empty Table to Hold Command Actions
					SCRIPT_CurrentCommandTable = {};
					
					while ((SCRIPT_CurrentSEPPos &lt; SCRIPT_CurrentScriptLength) and (SCRIPT_CurrentCmdAction ~= "") and (CancelActions ~= true)) do
						---Get Next Command Action (i.e. "ImgAdd" or "police.dff" and so on)---
						--Reset Variables
						SCRIPT_CurrentStartSEP = "";
						SCRIPT_CurrentEndSEP = "";
						--Find First | (SEP)
						SCRIPT_CurrentSEPPos = SCRIPT_CurrentSEPPos + 1
						SCRIPT_CurrentStartSEP = String.Find(SCRIPT_CurrentScriptCommand, "|", SCRIPT_CurrentSEPPos, false);
						if (SCRIPT_CurrentStartSEP ~= -1) then --If the first SEParator is found...
							if (SCRIPT_CurrentStartSEP &lt; SCRIPT_CurrentScriptLength) then
								--Find Second | (SEP)
								SCRIPT_CurrentSEPPos = SCRIPT_CurrentStartSEP + 1;
								SCRIPT_CurrentStopSEP = String.Find(SCRIPT_CurrentScriptCommand, "|", SCRIPT_CurrentSEPPos, false);
								if (SCRIPT_CurrentStopSEP == -1) then
									--Do nothing with current line. Move to next line
								else
									--Take off the | (SEPs) from command action
									SCRIPT_CurrentStartSEP = SCRIPT_CurrentStartSEP + 1;
									SCRIPT_CurrentStopSEP = SCRIPT_CurrentStopSEP - 1;
									--Find out the length of the current command action
									SCRIPT_CurrentSEPLength = SCRIPT_CurrentStopSEP - SCRIPT_CurrentStartSEP + 1;
									--Get the command action into a variable
									SCRIPT_CurrentCmdAction = String.Mid(SCRIPT_CurrentScriptCommand, SCRIPT_CurrentStartSEP, SCRIPT_CurrentSEPLength);
									--Add the currect action to the table
									SCRIPT_CurrentCommandTableColumn = SCRIPT_CurrentCommandTableColumn + 1;
									Table.Insert(SCRIPT_CurrentCommandTable, SCRIPT_CurrentCommandTableColumn, SCRIPT_CurrentCmdAction);
								end
							end
						end
					end
					Scripter_SavePrompt = false;
					---Determine what the current command is then add command actions to the table---
					--GIMIVer
					if (SCRIPT_CurrentCommandTable[1] == "GIMIVer") then
						--Add all info to the ScriptActions tables
						ScriptActions_TableNum = ScriptActions_TableNum + 1;
						Table.Insert(ScriptActions_1, ScriptActions_TableNum, SCRIPT_CurrentCommandTable[1]);
						Table.Insert(ScriptActions_2, ScriptActions_TableNum, SCRIPT_CurrentCommandTable[2]);
						Table.Insert(ScriptActions_3, ScriptActions_TableNum, "nil");
						Table.Insert(ScriptActions_4, ScriptActions_TableNum, "nil");
						Table.Insert(ScriptActions_5, ScriptActions_TableNum, "nil");
					end
