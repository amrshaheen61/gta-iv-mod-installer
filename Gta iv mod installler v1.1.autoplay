function GetLastPath ()
	--Determine game directory
	--Look for gimi.ini file
	gimiIniExists = File.DoesExist(gimiIniFile);
	--Check for error--
	if (gimiIniExists == false) then --If gimi.ini does NOT exist...
change_lang_message("Global_functions_texts","216","none","ok")
		Error901 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
		--Log Error
		ErrorLog_Value = text_message;
		ErrorLogUpdate();
		Application.Exit();
	else --If gimi.ini DOES exist
	--End error check--
	
		--Get LastUsedGamePath from INI
		LastUsedGamePath = INIFile.GetValue(gimiIniFile, "gimi", "LastUsedGamePath");
		if (LastUsedGamePath ~= "") then --If LastUsedGamePath is blank...
			--See if specified path exists
			LastUsedGamePathExists = File.DoesExist(LastUsedGamePath .. "\\gtaiv.exe");
			if (LastUsedGamePathExists ~= false) then --If LastUsedGamePath DOES exist...
				DetectedDirFinal = LastUsedGamePath;
		
			Paragraph.SetVisible("error_g", true);
	   Paragraph.SetVisible("error_r", false);
	   else --If LastUsedGamePath does NOT exist...
				--Assign default final game path variable
				DetectedDirFinal = DefaultGamePath;
				Paragraph.SetVisible("error_g", false);
	   Paragraph.SetVisible("error_r", true);
			end
		else
			--Get GTA IV game dir from registry
			DetectedDir = Registry.GetValue(HKEY_LOCAL_MACHINE, "Software\\Rockstar Games\\GTA IV \\Installation", "ExePath", true);
			--If game path is NOT detected, assign default game path
			if (DetectedDir == "") then
				DetectedDirFinal = DefaultGamePath;
			else
				--If game path IS detected, get DRIVE and FOLDER--
				--Remove the prentheses from detected game dir
				DetectedDir_Length = String.Length(DetectedDir);
				DetectedDir_ChopLeft = DetectedDir_Length - 2;
				DetectedDir_Trimmed = String.Mid(DetectedDir, 2, DetectedDir_ChopLeft);
				--Find out the DRIVE and FOLDER path
				DetectedDir_Table = String.SplitPath(DetectedDir_Trimmed);
				DetectedDir_Drive = DetectedDir_Table.Drive;
				DetectedDir_Folder = DetectedDir_Table.Folder;
				--Join the DRIVE and FOLDER together to get final dir
				DetectedDirComplete = String.Concat(DetectedDir_Drive, DetectedDir_Folder);
				--Make sure detected directory exists
				DetectedDirCompleteExists = File.DoesExist(DetectedDirComplete .. "\\gtaiv.exe");
				if (DetectedDirCompleteExists ~= false) then --If detected directory DOES exist...
					DetectedDirFinal = DetectedDirComplete;
				Paragraph.SetVisible("error_g", true);
	   Paragraph.SetVisible("error_r", false);
				else --If detected directory does NOT exist...
					--Assign default game path
					DetectedDirFinal = DefaultGamePath;
				Paragraph.SetVisible("error_g", false);
	   Paragraph.SetVisible("error_r", true);
				end
			end
		end
	end
end



