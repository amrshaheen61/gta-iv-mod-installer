			  --value to delimite delim char  whether to remove tabs\spaces (true\false value)
function Delimit(DelimitedString, Delimiter, Delim_RemoveTabSpace)
	tbReturn = {};
	tbReturnDelimit = {};
	local strWorking;
	local nPos = nil;
	local strData;
	local nTableIndex = 1;
	local nDelimiterLength = String.Length(Delimiter);
	
	if(nDelimiterLength &lt; 1)then
		tbReturn[nTableIndex] = DelimitedString;
		return tbReturn;
	end
	
	strWorking = DelimitedString;
	nPos = String.Find(strWorking,Delimiter, 1, false);
	while(nPos ~= -1)do
		strData = String.Left(strWorking,nPos-1);
		tbReturn[nTableIndex] = strData;
		nTableIndex = nTableIndex + 1;
		local nLength = String.Length(strWorking);
		strWorking = String.Right(strWorking,nLength - (nPos + (nDelimiterLength-1)));
		nPos = String.Find(strWorking,Delimiter);
	end
	if(strWorking ~= "")then
		tbReturn[nTableIndex] = strWorking;
	end
	
	if (Delim_RemoveTabSpace == true) then
		--Remove Tabs\Spaces
		tbReturnDelimit_Counter = 1;
		for i, tbReturnDelimit_Orig in pairs(tbReturn) do
			tbReturnDelimit_Val = RemoveTabSpace(tbReturnDelimit_Orig);
			strWorking = strWorkingNoTabSpace;
			tbReturnDelimit[tbReturnDelimit_Counter] = tbReturnDelimit_Val;
			tbReturnDelimit_Counter = tbReturnDelimit_Counter + 1;
		end
		tbReturn = tbReturnDelimit;
	end
	
	return tbReturn;
end




