<Script>change_lang ("Paragraph2","56","Paragraph");
change_lang ("error_r","57","Paragraph");
change_lang ("error_g","58","Paragraph");
change_lang ("Paragraph1","63","Paragraph");


change_lang ("install-dir_browse","59","Button");
change_lang ("Button1","60","Button");
change_lang ("install_buttons","61","Button");
change_lang ("Button3","62","Button");

change_lang ("gamedir_inputbox","inputbox","Input");


change_lang ("install-dir_inputbox","inputbox","Input");
change_lang ("mod-location-dir_inputbox","inputbox","Input");

--Get LastUsedGamePath From INI File
GetLastPath();

--Run function to determine backup directory
AssignModBackupDir();

--Assign Functions
function GetBackupList ()
	--Assign Variables
	GetBackupList_BackupsList = {};
	--Clear Existing Data In Listbox
	ListBox.DeleteItem("backup_list", -1);
	
	--Run function to determine backup directory
	AssignModBackupDir();
	
	--Find all GIMI compatible backups in game directory
	GetBackupList_FoldersFound = Folder.Find(ModBackupDir, "*", false, nil);	
	if (GetBackupList_FoldersFound == nil) then
		--No folders were found
change_lang_message("page_BackupRestore_texts","63","none","ok")
		Error39393 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
	else
		--Count how many files were found
		GetBackupList_FolderCount = Table.Count(GetBackupList_FoldersFound);
		
		--Make sure .GIMI file exists in folder. If not, remove the folder from the folder table
		GetBackupList_FolderCounter = 0;
		while (GetBackupList_FolderCounter &lt; GetBackupList_FolderCount) do
			--Re-count how many files were found
			GetBackupList_FolderCount = Table.Count(GetBackupList_FoldersFound);
			--Get Next Folder
			GetBackupList_FolderCounter = GetBackupList_FolderCounter + 1;
			GetBackupList_CurrentFolder = GetBackupList_FoldersFound[GetBackupList_FolderCounter];
			--See if .GIMI file exists
			GetBackupList_ScriptExists = File.DoesExist(GetBackupList_CurrentFolder .. "\\backup." .. ScriptExt);
			if (GetBackupList_ScriptExists ~= true) then
				--No GIMI File Exists.
			else
				--GIMI File DOES Exist
				--Parse subfolders to get the last subfolder only
				GetBackupList_BackslashPos = String.ReverseFind(GetBackupList_CurrentFolder, "\\", false);
				if (GetBackupList_BackslashPos ~= -1) then
					--There IS at least one backslash
					--Remove ending backslashes (if there are any)
					RemoveEndSlash_Value = GetBackupList_CurrentFolder;
					RemoveEndSlash();
					GetBackupList_CurrentFolder = RemoveEndSlash_Value;
					--Count string characters
					GetBackupList_StringCount = String.Length(GetBackupList_CurrentFolder);
					GetBackupList_BackslashPos2 = String.ReverseFind(GetBackupList_CurrentFolder, "\\", false);
					GetBackupList_DirNameCount = GetBackupList_StringCount - GetBackupList_BackslashPos2;
					--Get info after the last backslash (the name of the folder to be renamed)
					GetBackupList_DirNameOnly = String.Right(GetBackupList_CurrentFolder, GetBackupList_DirNameCount);
					
					--Insert to listbox
					GetBackupList_Inserted0303 = ListBox.InsertItem("backup_list", -1, GetBackupList_DirNameOnly, GetBackupList_CurrentFolder);
				end			
			end
		end
	end
end




--Assign input box with game directory
GameDir = DetectedDirFinal;
--Verify GameDir is correct
VerifyGameDir();
--If the Game Directory IS correct
if (VerifyGameDir_Result == true) then
	Input.SetText("gamedir_inputbox", DetectedDirFinal);
	GetBackupList();
end
GameDir = "";



