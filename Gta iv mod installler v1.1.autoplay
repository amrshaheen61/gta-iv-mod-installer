		function Backup_ImgAdd ()
			
			--Parse "file to add" to get filename only
			Backup_ImgAdd_FilenameTable = String.SplitPath("C:\\" .. ScriptActions_2_Current);
			Backup_ImgAdd_FilenameOnly = Backup_ImgAdd_FilenameTable.Filename .. Backup_ImgAdd_FilenameTable.Extension;
			Backup_ImgAdd_SubfolderOnly = Backup_ImgAdd_FilenameTable.Folder;
			
			--Parse Img File Path to get IMG File Name only
			Backup_ImgAdd_ImgNameTable = String.SplitPath("C:\\" .. ScriptActions_3_Current);
			Backup_ImgAdd_ImgNameOnly = Backup_ImgAdd_ImgNameTable.Filename .. Backup_ImgAdd_ImgNameTable.Extension;
			
			--See if the specified file already exists in the img file
			ImgExtractFile_Filename = Backup_ImgAdd_FilenameOnly
			ImgExtractFile_ImgFile	= ScriptActions_3_Current
			ImgExtractFile();
		
			if (ImgExtractFile_Exists == true) then
				--The file DOES exist in the IMG archive
				--Move the file to the backup folder
				Folder.Create(CurrentModBackupDir .. "\\" .. Backup_ImgAdd_ImgNameOnly);
