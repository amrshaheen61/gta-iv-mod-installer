	if (Scripter_EditType == "add") then
		--Count how many commands are to be inserted
		Scripter_CommandInsert_Value_Count = Table.Count(Scripter_CommandInsert_Value);
		Scripter_CommandInsert_Value_Counter = 0;
		--Add each command
		while (Scripter_CommandInsert_Value_Counter &lt; Scripter_CommandInsert_Value_Count) do
			Scripter_CommandInsert_Value_Counter = Scripter_CommandInsert_Value_Counter + 1;
			Scripter_CommandInsert_Result = ListBox.AddItem("commandlist", Scripter_CommandInsert_Value[Scripter_CommandInsert_Value_Counter], Scripter_CommandInsert_Data);
			--Select Command
			ListBox.SelectItem("commandlist", Scripter_CommandInsert_Result);
			Scripter_SavePrompt = true;
		end
	end
	--If Modifying an existing command
	if (Scripter_EditType == "modify") then
		--Count how many commands are to be inserted
		Scripter_CommandInsert_Value_Count = Table.Count(Scripter_CommandInsert_Value);
		Scripter_CommandInsert_Value_Counter = 0;
		while (Scripter_CommandInsert_Value_Counter &lt; Scripter_CommandInsert_Value_Count) do
			Scripter_CommandInsert_Value_Counter = Scripter_CommandInsert_Value_Counter + 1;
			--Add New Command Value
			Scripter_CommandInsert_Result = ListBox.InsertItem("commandlist", Scripter_Modify_CommandPosition, Scripter_CommandInsert_Value[Scripter_CommandInsert_Value_Counter], Scripter_CommandInsert_Data);
			--Remove Existing Command Value
			Scripter_Modify_CommandDeletePosition = Scripter_Modify_CommandPosition + 1;
			ListBox.DeleteItem("commandlist", Scripter_Modify_CommandDeletePosition);
			--Select Command
			ListBox.SelectItem("commandlist", Scripter_CommandInsert_Result);
			Scripter_SavePrompt = true;
		end
	end
	
	--Reset Variables
	Scripter_Modify_CommandPosition = nil;
	Scripter_Modify_CommandDeletePosition = nil;
	Scripter_CommandInsert_Result = nil;
	
end
