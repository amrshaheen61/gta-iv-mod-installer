function Scripter_VerifyEditableCommand()
	--Verify that the currently selected command is editable
	Scripter_VerifyCommand_Selected = ListBox.GetSelected("commandlist");
	if (Scripter_VerifyCommand_Selected == nil) then
		--No Command Is Selected
change_lang_message("page_Scripter{1}_texts","94","none","ok")
		result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
	else
	Scripter_VerifyCommand = ListBox.GetItemData("commandlist", Scripter_VerifyCommand_Selected [1]);
	if (Scripter_VerifyCommand == "GIMIVer") then
		--Don't Allow Editing of GIMI Version
change_lang_message("page_Scripter{1}_texts","93","none","ok")
		result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
		Scripter_CanProceed = false;
	else
		Scripter_CanProceed = true;
	end
end
end

