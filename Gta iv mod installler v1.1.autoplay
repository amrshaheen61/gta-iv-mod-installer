--Check to make sure text line exists
function TextLineExists ()
	--Reset Variables
	TextLineExists_Result = false;
	TextLineExists_LineNum = "";
	
	--Read text file to table
	TextLineExists_ReadtoTable = TextFile.ReadToTable(TextLineExists_Filename);
	if (TextLineExists_ReadtoTable == nil) then
		--Could not read text file to a table
		TextLineExists_Result = false;
	else
		--Search text table to see if the specified line exists
		TextLineExists_Counter = 0;
		--Count Lines In Text File
		TextLineExists_Count = Table.Count(TextLineExists_ReadtoTable);
		while ((TextLineExists_Counter &lt; TextLineExists_Count) and (TextLineExists_Result ~= true)) do
			--Move to Next Line
			TextLineExists_Counter = TextLineExists_Counter + 1;
			TextLineExists_CurrentLine = TextLineExists_ReadtoTable[TextLineExists_Counter];
			--See if current line is the correct one
			TextLineExists_IsCurrentLine = String.Find(TextLineExists_CurrentLine, TextLineExists_Line, 1, false);
			if (TextLineExists_IsCurrentLine ~= -1) then
				--It IS the correct string
				TextLineExists_Result = true;
				TextLineExists_LineNum = TextLineExists_Counter;
			else
				--It is NOT the correct string
					--Do nothing. Result will remain false
			end
		end
	end 
	
	--Reset Variables
	TextLineExists_Filename = "";
	TextLineExists_Line = "";
end --function TextLineExists ()


