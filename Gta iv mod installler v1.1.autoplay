function ImgScript_Backup ()
	-----------------------------------------------------------
	--It's called "Backup" but is actually used for "install"
	------------------------------------------------------------

	--Required Input Variables: ImgScript_Backup_TableValue_Input, ImgScript_Backup_TableFile_Input
	
	--Assign Variables based on current input
	ImgScript_Backup_TableValue = ImgScript_Backup_TableValue_Input;
	ImgScript_Backup_TableFile = ImgScript_Backup_TableFile_Input;
	
	--Assign Variables
	ImgScript_Backup_StopLoop = false;

	--See if "-open....filename.img" already exists in table
	ImgScript_Backup_Counter = 0;
	ImgScript_Backup_TableCount = Table.Count(ImgScript_Backup_Table);
	
	if (ImgScript_Backup_TableCount == 0) then
		--The table is empty
		ImgScript_Backup_InsertPos = ImgScript_Backup_TableCount + 1;
		--Add info to the table
		Table.Insert(ImgScript_Backup_Table, ImgScript_Backup_InsertPos, "-open \"" .. DetectedDirFinal .. "\\" .. ImgScript_Backup_TableFile .. "\"");
		ImgScript_Backup_InsertPos = ImgScript_Backup_InsertPos + 1;
		Table.Insert(ImgScript_Backup_Table, ImgScript_Backup_InsertPos, ImgScript_Backup_TableValue_Input);
		ImgScript_Backup_InsertPos = ImgScript_Backup_InsertPos + 1;
		Table.Insert(ImgScript_Backup_Table, ImgScript_Backup_InsertPos, "-close");
	else
		--The table is NOT empty
		--See if specified IMG file is already in the script as "-open....filename.img"
		while ((ImgScript_Backup_Counter &lt; ImgScript_Backup_TableCount) and (ImgScript_Backup_StopLoop ~= true)) do
			ImgScript_Backup_Counter = ImgScript_Backup_Counter + 1;
			ImgScript_Backup_CurrentLine = ImgScript_Backup_Table[ImgScript_Backup_Counter];
			--Search for "filename.img" in current line
			ImgScript_Backup_ImgNameSearch = String.Find(ImgScript_Backup_CurrentLine, ImgScript_Backup_TableFile_Input, 1, false);
			if (ImgScript_Backup_ImgNameSearch == -1) then
				--It is NOT the current line
			else
				--It IS the current line
				ImgScript_Backup_StopLoop = true;
				--Insert info to table right after "-open....filename.img"
				ImgScript_Backup_InsertPos = ImgScript_Backup_Counter + 1;
				Table.Insert(ImgScript_Backup_Table, ImgScript_Backup_InsertPos, ImgScript_Backup_TableValue_Input);
			end
		end
		if (ImgScript_Backup_StopLoop ~= true) then
			--The specified img file does NOT already exist in the table
			ImgScript_Backup_InsertPos = ImgScript_Backup_TableCount + 1;
			--Add new info to the end of the table
			Table.Insert(ImgScript_Backup_Table, ImgScript_Backup_InsertPos, "-open \"" .. DetectedDirFinal .. "\\" .. ImgScript_Backup_TableFile .. "\"");
			ImgScript_Backup_InsertPos = ImgScript_Backup_InsertPos + 1;
			Table.Insert(ImgScript_Backup_Table, ImgScript_Backup_InsertPos, ImgScript_Backup_TableValue_Input);
			ImgScript_Backup_InsertPos = ImgScript_Backup_InsertPos + 1;
			Table.Insert(ImgScript_Backup_Table, ImgScript_Backup_InsertPos, "-close");	
		end
	end
	
	--Reset Variables
	ImgScript_Backup_TableValue_Input = "";
	ImgScript_Backup_TableFile_Input = "";
	--Add all info to script file
	TextFile.WriteFromTable(BackupImgScript, ImgScript_Backup_Table, false);
end





