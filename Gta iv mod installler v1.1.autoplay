function IsNumeric (IsNumeric_Value)
	if (IsNumeric_Value == "0") then
		return true;
	else
		IsNumeric_Test = String.ToNumber(IsNumeric_Value);
		if (IsNumeric_Test == 0) then
			return false;
		else
			return true;
		end
	end
end

