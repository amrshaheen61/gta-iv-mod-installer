function Scripter_Modify_Actions ()
	--Get selected command
	Scripter_Modify_SelectedCmdTable = ListBox.GetSelected("commandlist");
	if (Scripter_Modify_SelectedCmdTable == nil) then
		--No command is selected
change_lang_message("page_Scripter{1}_texts","94","none","ok")
		result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
	else
		Scripter_Modify_CurrentSelected = Scripter_Modify_SelectedCmdTable[1];
