<Event>
<Name>On Click</Name>
<Args/>
<Script>--Disable Continue Button
Button.SetEnabled("continue_button", false);
change_lang_message("page_Multiple_WFT_texts","21","none","none")
Button.SetText("continue_button", text_message);

--See if wft file is selected
MultiList_wft_Selected = ListBox.GetSelected("wft_file_list");
if (MultiList_wft_Selected == nil) then
	--Nothing is selected
change_lang_message("page_Multiple_WFT_texts","19","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
else
	--Get data (full path and filename) for selected item
	wftPath = ListBox.GetItemData("wft_file_list", MultiList_wft_Selected[1]);
	--Parse filepath to get filename only
	MultiList_wft_ParsedPath = String.SplitPath(wftPath);
	wftFile = MultiList_wft_ParsedPath.Filename .. MultiList_wft_ParsedPath.Extension;
	
	--See if wtd file is selected
	MultiList_wtd_Selected = ListBox.GetSelected("wtd_file_list");
	if (MultiList_wtd_Selected == nil) then
		--Nothing is selected
change_lang_message("page_Multiple_WFT_texts","20","none","ok")
		result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
	else
		--Get data (full path and filename) for selected item
		wtdPath = ListBox.GetItemData("wtd_file_list", MultiList_wtd_Selected[1]);
		--Parse filepath to get filename only
		MultiList_wtd_ParsedPath = String.SplitPath(wtdPath);
		wtdFile = MultiList_wtd_ParsedPath.Filename .. MultiList_wtd_ParsedPath.Extension;
		Button.SetEnabled("continue_button", true);
change_lang_message("page_Multiple_WFT_texts","22","none","none")
        Button.SetText("continue_button", text_message);
		
		--Go to Vehicle Info Page
		Page.Jump("Vehicle Info");
	end
end

--Enable Continue Button
Button.SetEnabled("continue_button", true);
change_lang_message("page_Multiple_WFT_texts","22","none","none")
Button.SetText("continue_button", text_message);</Script>
