<Event>
<Name>On Click</Name>
<Args/>
<Script>--Assign Variables
CancelActions = false;


----Assemble the command together based on values from inputs----

--++Get all input values++--
--inputbox1
inputbox1_Visibility = Input.IsVisible("inputbox1");
if (inputbox1_Visibility == true) then
	inputbox1_value = Input.GetText("inputbox1");
	if (inputbox1_value == "") then
		CancelActions = true;
		Error = Dialog.Message("Error", "The first input box can not be empty.", MB_OK, MB_ICONSTOP, MB_DEFBUTTON2);
	end
end
--inputbox2
inputbox2_Visibility = Input.IsVisible("inputbox2");
if (inputbox2_Visibility == true) then
	inputbox2_value = Input.GetText("inputbox2");
	if (inputbox2_value == "") then
		CancelActions = true;
		Error = Dialog.Message("Error", "The second input box can not be empty.", MB_OK, MB_ICONSTOP, MB_DEFBUTTON2);
	end
end
--inputbox3
inputbox3_Visibility = Input.IsVisible("inputbox3");
if (inputbox3_Visibility == true) then
	inputbox3_value = Input.GetText("inputbox3");
	if (inputbox3_value == "") then
		CancelActions = true;
		Error = Dialog.Message("Error", "The third input box can not be empty.", MB_OK, MB_ICONSTOP, MB_DEFBUTTON2);
	end
end
--listbox1
listbox1_Visibility = ListBox.IsVisible("listbox1");
if (listbox1_Visibility == true) then
	listbox1selectedTable = ListBox.GetSelected("listbox1");
	if (listbox1selectedTable == nil) then
		--Nothing is selected
		CancelActions = true;
		Error = Dialog.Message("Error", "You must select one of the options in the first list box.", MB_OK, MB_ICONSTOP, MB_DEFBUTTON2);
	else
		listbox1selected = listbox1selectedTable[1];
		listbox1_value = ListBox.GetItemData("listbox1", listbox1selected);
	end
end
--listbox2
listbox2_Visibility = ListBox.IsVisible("listbox2");
if (listbox2_Visibility == true) then
	listbox2selectedTable = ListBox.GetSelected("listbox2");
	if (listbox2selectedTable == nil) then
		--Nothing is selected
		CancelActions = true;
		Error = Dialog.Message("Error", "You must select one of the options in the second list box.", MB_OK, MB_ICONSTOP, MB_DEFBUTTON2);
	else
		listbox2selected = listbox2selectedTable[1];
		listbox2_value = ListBox.GetItemData("listbox2", listbox2selected);
	end
end


--++Assemble the command++--
if (CancelActions ~= true) then
	--Reset Variable
	Scripter_AssembledCommand = "";
	Scripter_CommandInsert_Value = {};
	Scripter_CommandInsert_Data = "";
	
	--ModName, ModInfo, etc. Commands
	if ((DC_ActionTable[1] == "ModName") or (DC_ActionTable[1] == "ModDesc") or (DC_ActionTable[1] == "ModVer") or (DC_ActionTable[1] == "ModCreator") or (DC_ActionTable[1] == "ModEmail") or (DC_ActionTable[1] == "ModURL") or (DC_ActionTable[1] == "ModExtraInfo")) then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. inputbox1_value .. "|";
	end
	--ImgAdd
	if (DC_ActionTable[1] == "ImgAdd") then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. inputbox1_value .. "|" .. inputbox2_value .. "|";
	end
	--ImgDelete
	if (DC_ActionTable[1] == "ImgDelete") then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. inputbox1_value .. "|" .. inputbox2_value .. "|";
	end
	--ImgExtract
	if (DC_ActionTable[1] == "ImgExtract") then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. inputbox1_value .. "|" .. inputbox2_value .. "|" .. inputbox3_value .. "|";
	end
	--FileRun
	if (DC_ActionTable[1] == "FileRun") then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. listbox1_value .. "|" .. inputbox1_value .. "|" .. listbox2_value .. "|";
	end
	--FileAdd
	if (DC_ActionTable[1] == "FileAdd") then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. inputbox1_value .. "|" .. listbox1_value .. "|" .. inputbox2_value .. "|" .. listbox2_value .. "|";
	end
	--FileDelete
	if (DC_ActionTable[1] == "FileDelete") then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. listbox1_value .. "|" .. inputbox1_value .. "|" .. listbox2_value .. "|";
	end
	--FileRename
	if (DC_ActionTable[1] == "FileRename") then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. listbox1_value .. "|" .. inputbox1_value .. "|" .. inputbox2_value .. "|" .. listbox2_value .. "|";
	end
	--FileCopy
	if (DC_ActionTable[1] == "FileCopy") then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. listbox1_value .. "|" .. inputbox1_value .. "|" .. inputbox2_value .. "|" .. listbox2_value .. "|";
	end
	--FileMove
	if (DC_ActionTable[1] == "FileMove") then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. listbox1_value .. "|" .. inputbox1_value .. "|" .. inputbox2_value .. "|" .. listbox2_value .. "|";
	end
	--InsertText
	if (DC_ActionTable[1] == "InsertText") then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. inputbox1_value .. "|" .. inputbox2_value .. "|" .. listbox1_value .. "|" .. inputbox3_value .. "|";
	end
	--DeleteText
	if (DC_ActionTable[1] == "DeleteText") then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. inputbox1_value .. "|" .. inputbox2_value .. "|" .. listbox1_value .. "|";
	end
	--ReplaceText
	if (DC_ActionTable[1] == "ReplaceText") then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. inputbox1_value .. "|" .. inputbox2_value .. "|" .. inputbox3_value .. "|" .. listbox1_value .. "|";
	end
	--CreateDir
	if (DC_ActionTable[1] == "CreateDir") then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. listbox1_value .. "|" .. inputbox1_value .. "|";
	end
	--DeleteDir
	if (DC_ActionTable[1] == "DeleteDir") then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. listbox1_value .. "|" .. inputbox1_value .. "|" .. listbox2_value .. "|";
	end
	--RenameDir
	if (DC_ActionTable[1] == "RenameDir") then
		Scripter_AssembledCommand = "|" .. DC_ActionTable[1] .. "|" .. listbox1_value .. "|" .. inputbox1_value .. "|" .. inputbox2_value .. "|" .. listbox2_value .. "|";
	end
	
	--Allow command to be added to the main list
	if (Scripter_AssembledCommand ~= "") then
		Scripter_CommandInsert = true;
		Scripter_CommandInsert_Value[1] = Scripter_AssembledCommand;
		Scripter_CommandInsert_Data = DC_ActionTable[1];
		Page.Jump("Scripter_Commands");
	end
end</Script>
