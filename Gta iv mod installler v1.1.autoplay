function VerifyLineHAN (VerifyLineHAN_Value_Table,VerifyLineHAN_HandlingName_String)
	--Assign Variables
	VerifyLineHAN_Return = {}; --Returns a table containing all verified handling lines
	VerifyLineHAN_Counter = 0;
	
	--Count How Many Handline Lines Were Specified
	VerifyLineHAN_Value_Table_Count = Table.Count(VerifyLineHAN_Value_Table);
	while (VerifyLineHAN_Counter &lt; VerifyLineHAN_Value_Table_Count) do
		VerifyLineHAN_Counter = VerifyLineHAN_Counter + 1;
		--Get Next Handling Line That Is to Be Verified
		VerifyLineHAN_CurrentLineFull = VerifyLineHAN_Value_Table[VerifyLineHAN_Counter];
		

		-------------------------Run All Verification Proceedures-----------------------------
		
		--See Where the Handling Name Is In Current Line (i.e. "CHEETAH")
		VerifyLineHAN_NamePos = String.Find(VerifyLineHAN_CurrentLineFull, VehicleIDE_Handling, 1, true);
		VerifyLineHAN_CurrentLineFull_Length = String.Length(VerifyLineHAN_CurrentLineFull);

		if (VerifyLineHAN_NamePos ~= -1) then
			--Get New Line and Remove Info to Left of Handling Name (i.e. "CHEETAH")
			VerifyLineHAN_NameCharsToGet = VerifyLineHAN_CurrentLineFull_Length - VerifyLineHAN_NamePos + 1;
			VerifyLineHAN_CurrentLine_New = String.Right(VerifyLineHAN_CurrentLineFull, VerifyLineHAN_NameCharsToGet);

			--Remove All Tabs and Double Spaces
			VerifyLineHAN_CurrentLineCleaned = RemoveTabDoubleSpace(VerifyLineHAN_CurrentLine_New);
			--Delimit Handling Line
			VerifyLineHAN_CurrentLine_Table = Delimit(VerifyLineHAN_CurrentLineCleaned, " ", false);
			--Make Sure Handling Name Has No Extra Characters (i.e. "CHEETAHE")
			VerifyLineHAN_ExtraCharCheck = VerifyLineHAN_CurrentLine_Table[1];
			VerifyLineHAN_ExtraCharCheck_Cleaned = RemoveTabSpace(VerifyLineHAN_ExtraCharCheck);
			if (VerifyLineHAN_ExtraCharCheck_Cleaned ~= VehicleIDE_Handling) then
				--Handling Name Has Extra Characters (i.e. "CHEETAHEE")
			else
				--Count Fields In Current Handling Line
				VerifyLineHAN_CurrentLine_Table_Count = Table.Count(VerifyLineHAN_CurrentLine_Table);
				
				--Count Fields In ORIGINAL Handling Line
				VerifyLineHAN_Original_FieldCount = Table.Count(VehicleHAN_Original_Delim);
				
				if (VerifyLineHAN_CurrentLine_Table_Count ~= VerifyLineHAN_Original_FieldCount) then
					--The Line Is Invalid (Not Enough Fields As Original)
				else
					
					--Assemble Final Handling Line
					VerifyLineHAN_FinalLine = "";
					VerifyLineHAN_Assemble_Counter = 0;
					while (VerifyLineHAN_Assemble_Counter &lt; VerifyLineHAN_CurrentLine_Table_Count) do
						VerifyLineHAN_Assemble_Counter = VerifyLineHAN_Assemble_Counter + 1;
						--Get Next "Piece" of Handling Data
						VerifyLineHAN_Assemble_CurrentPiece = VerifyLineHAN_CurrentLine_Table[VerifyLineHAN_Assemble_Counter];
						if (VerifyLineHAN_Assemble_Counter == 1) then
							--Insert Some Spaces After Handling Name (i.e. "CHEETAH     ")
							VerifyLineHAN_FinalLine = VerifyLineHAN_Assemble_CurrentPiece .. "     ";
						else
							--Add current piece to the final line
							VerifyLineHAN_FinalLine = VerifyLineHAN_FinalLine .. " " .. VerifyLineHAN_Assemble_CurrentPiece;
						end
					end
					
					if (VerifyLineHAN_FinalLine ~= "") then
						--Add Current Line to Return Table
						VerifyLineHAN_Return_Count = Table.Count(VerifyLineHAN_Return);
						VerifyLineHAN_Return_InsertPos = VerifyLineHAN_Return_Count + 1;
						VerifyLineHAN_Return[VerifyLineHAN_Return_InsertPos] = VerifyLineHAN_FinalLine;
					end
				end
			end
		end
	end
	--Return Table Containing All Valid Handling Lines Only
	return VerifyLineHAN_Return;
end



