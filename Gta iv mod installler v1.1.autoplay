<Event>
<Name>On Show</Name>
<Args/>
<Script>change_lang ("retry_button","68","Button");
change_lang ("back_button","69","Button");

change_lang ("update_text","up","Paragraph_no");

--Assign Variables
CancelUpdate = false;
ErrorCheck = "";
InstallUpdate = false;
LaunchUpdateURL = false;
GIMI_UpdateSite = nil;
GIMI_UpdateFile = nil;
GIMI_UpdateType = nil;
GIMI_UpdateVer = nil;

--Clear Existing Update Status Text
Paragraph.SetText("update_text", "");
--Change Retry button to temporary update text
Button.SetEnabled("retry_button", false);
change_lang_message("page_update_texts","70","none","none")
Button.SetText("retry_button", text_message);
--Disable Back Button
Button.SetEnabled("back_button", false);

--Make sure the INI file exists
INIExists = File.DoesExist(gimiIniFile);
if (INIExists ~= true) then
	CancelUpdate = true;
	--Update text status message
	ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","71",ExistingUpdateText,"ok")
	Paragraph.SetText("update_text",text_message);
	--Log Error
change_lang_message("page_update_texts","71","","ok")
	TextFile.WriteFromString(ErrorLog, text_message, true);
else
	if (CancelUpdate ~= true) then
		--Get update URL location from ini file
		UpdateURL = INIFile.GetValue(gimiIniFile, "Update", "ScriptURL");
		--Make sure there was no problem getting URL from ini file
		if (UpdateURL == "") then --if there WAS an error...
			CancelUpdate = true;
			--Update text status message
			ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","72",ExistingUpdateText,"ok")
			Paragraph.SetText("update_text", text_message);
			--Log Error
change_lang_message("page_update_texts","72","","ok")
	         TextFile.WriteFromString(ErrorLog, text_message, true);		
	         else
			GIMI_SCRIPT_URL = UpdateURL;
		end
		
		--Get connection test URL location from ini file
		TestURL = INIFile.GetValue(gimiIniFile, "Update", "TestURL");
		--Make sure there was no problem getting URL from ini file
		if (TestURL == "") then --if there WAS an error...
			CancelUpdate = true;
			--Update text status message
			ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","73",ExistingUpdateText,"ok")
			Paragraph.SetText("update_text",text_message);
change_lang_message("page_update_texts","73","","ok")
	         TextFile.WriteFromString(ErrorLog, text_message, true);	
	         	else
			CONNECTION_TEST_URL = TestURL;
		end
		
		
		--See if it can connect to and find the update script
		if (CancelUpdate ~= true) then
			--Update text status message
			ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","74",ExistingUpdateText,"ok")
			Paragraph.SetText("update_text", text_message);
			--Test connection to internet
			GimiUpdateScriptVerify = HTTP.TestConnection(CONNECTION_TEST_URL, 20, 80, nil, nil);
			if (GimiUpdateScriptVerify == false) then
				CancelUpdate = true;
				--Update text status message
				ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","75",ExistingUpdateText,"ok")
				Paragraph.SetText("update_text", text_message);
change_lang_message("page_update_texts","75","","ok")
	            TextFile.WriteFromString(ErrorLog, text_message, true);
	            else
				--Create Update folder
				Folder.Create(_SourceFolder .. "\\update");
				--Empty the update folder
			File.Delete(_SourceFolder .. "\\update\\*.*", true, false, true, nil);
				--Update text status message
				ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","76",ExistingUpdateText,"ok")
				Paragraph.SetText("update_text", text_message);
				--Perform script download
				HTTP.Download(GIMI_SCRIPT_URL, _SourceFolder .. "\\update\\gimi.txt", MODE_BINARY, 20, 80, nil, nil, nil);
				--See if there was an error
				ErrorCheck = Application.GetLastError();
				if (ErrorCheck ~= 0) then --if there was a problem downloading the script...
					CancelUpdate = true;
					--Update text status message
					ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","77",ExistingUpdateText,"ok")
					Paragraph.SetText("update_text", text_message);
					--Log Error
change_lang_message("page_update_texts","77","","ok")
					TextFile.WriteFromString(ErrorLog, text_message, true);
				else
					if (CancelUpdate ~= true) then --If the script was downloaded successfully
					--Update text status message
					ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","78",ExistingUpdateText,"ok")
					Paragraph.SetText("update_text", text_message);
						--Execute update script to get download file info
						Application.RunScriptFile(_SourceFolder .. "\\update\\gimi.txt");
						if ((GIMI_UpdateSite == nil) or (GIMI_UpdateFile == nil) or (GIMI_UpdateType == nil) or (GIMI_UpdateVer == nil)) then --If variables can't be found
							CancelUpdate = true;
							--Update text status message
							ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","79",ExistingUpdateText,"ok")
							Paragraph.SetText("update_text", text_message);
							--Log Error
change_lang_message("page_update_texts","79","","ok")
							TextFile.WriteFromString(ErrorLog, text_message, true);
						else
							if (CancelUpdate ~= true) then
								--Compare new version to current version of GIMI
								if (GIMI_UpdateVer &gt; GIMIVersion) then --If the update is newer...
									--Either "file" or "url"
									if (GIMI_UpdateType == "url") then --If updating externally...
										--Assign variable for retry button
										LaunchUpdateURL = true;
										--Update text status message
										ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","80",ExistingUpdateText,"ok")
										Paragraph.SetText("update_text", text_message);
										--Enable InstallUpdate variable (for use with the button)
										Button.SetEnabled("retry_button", true);
										--Change button text
change_lang_message("page_update_texts","81","none","none")
										Button.SetText("retry_button", text_message);
									end
									if (GIMI_UpdateType == "file") then --If updating internally...
										--Assign local file location (place to download file to)
										UpdateFileLocation = _SourceFolder .. "\\update\\" .. GIMI_UpdateFile;
										--Update text status message
										ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","82",ExistingUpdateText,"ok")
										Paragraph.SetText("update_text",text_message);
										--Show status dialog
										StatusDlg.Show(MB_ICONINFORMATION, false);
										--Download file
										HTTP.Download(GIMI_UpdateSite .. GIMI_UpdateFile, UpdateFileLocation, MODE_BINARY, 20, 80, nil, nil, nil);
										--Hide Status Dialog
										StatusDlg.Hide();
										--See if there was an error
										ErrorCheck = Application.GetLastError();
										if (ErrorCheck ~= 0) then --if there was a problem downloading the file...
											CancelUpdate = true;
											--Update text status message
											ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","83",ExistingUpdateText,"ok")
											Paragraph.SetText("update_text", text_message);
											--Log Error
change_lang_message("page_update_texts","83","","ok")
											TextFile.WriteFromString(ErrorLog, text_message, true);
											--Empty the update folder
											File.Delete(_SourceFolder .. "\\update\\*.*", true, false, true, nil);
											--Hide Status Dialog
											StatusDlg.Hide();
										else
											--Hide Status Dialog
											StatusDlg.Hide();
											if (CancelUpdate ~= true) then
												--Find out file type (zip, exe)
												UpdateFilePathTable = String.SplitPath(UpdateFileLocation);
												if (UpdateFilePathTable.Extension == ".zip") then --if it's a zip file...
													--Delete any existing files in extracted folder
													File.Delete(_SourceFolder .. "\\update\\extracted\\*.*", true, false, true, nil);
													--Update text status message
													ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","84",ExistingUpdateText,"ok")
													Paragraph.SetText("update_text", text_message);											
													--Extract Files
													Zip.Extract(UpdateFileLocation, {"*.*"}, _SourceFolder .. "\\update\\extracted", true, true, "", ZIP_OVERWRITE_ALWAYS, nil);
													--See if there was an error
													ErrorCheck = Application.GetLastError();
													if (ErrorCheck ~= 0) then --if there was a problem downloading the file...
														CancelUpdate = true;
														--Empty the update folder
														File.Delete(_SourceFolder .. "\\update\\*.*", true, false, true, nil);
														--Update text status message
														ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","85",ExistingUpdateText,"ok")
										             	Paragraph.SetText("update_text", text_message);
											            --Log Error
change_lang_message("page_update_texts","85","","ok")
											            TextFile.WriteFromString(ErrorLog, text_message, true);
															else
														--Find exe update file that was in the ZIP file
														UpdateExeFilesFound = File.Find(_SourceFolder .. "\\update\\extracted", "*.exe", false, false, nil);
														--Check for error--
														if (UpdateExeFilesFound == nil) then
															--Update text status message
change_lang_message("page_update_texts","86",ExistingUpdateText,"ok")
										             	Paragraph.SetText("update_text", text_message);
											            --Log Error
change_lang_message("page_update_texts","86","","ok")
											            TextFile.WriteFromString(ErrorLog, text_message, true);														
											            CancelUpdate = true;
														else
														--End error check--
															UpdateFileLocation = UpdateExeFilesFound[1];
															--Update text status message
															ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","87",ExistingUpdateText,"ok")
															Paragraph.SetText("update_text", text_message);
															--Enable InstallUpdate variable (for use with the button)
															InstallUpdate = true;
															Button.SetEnabled("retry_button", true);
															--Change button text
change_lang_message("page_update_texts","88",ExistingUpdateText,"ok")
													Button.SetText("retry_button", text_message);
														end
													end
												end
												if (UpdateFilePathTable.Extension == ".exe")then --if it's an exe file...
													--Update text status message
													ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","87",ExistingUpdateText,"ok")
													Paragraph.SetText("update_text", text_message);                 
													--Change button text
change_lang_message("page_update_texts","88",ExistingUpdateText,"ok")
													Button.SetText("retry_button", text_message);
													--Enable InstallUpdate variable (for use with the button)
													InstallUpdate = true;
													Button.SetEnabled("retry_button", true);
												end
												if ((UpdateFilePathTable.Extension ~= ".exe") and (UpdateFilePathTable.Extension ~= ".zip"))then --if it's not a ZIP or EXE file...
													--Update text status message
													ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","89",ExistingUpdateText,"ok")
													Paragraph.SetText("update_text", text_message);										
													--Log Error
change_lang_message("page_update_texts","89","","ok")
													TextFile.WriteFromString(ErrorLog,text_message, true);
													CancelUpdate = true;
												end
											end	
										end
									end
								else
									--Update text status message
									ExistingUpdateText = Paragraph.GetText("update_text");
change_lang_message("page_update_texts","90",ExistingUpdateText,"ok")
									Paragraph.SetText("update_text", text_message);										
									CancelUpdate = true;
								end
							end
						end
					--------
					end
				end
				ErrorCheck = "";
			end
		end
	end
end

--Change Retry button back to normal
if ((InstallUpdate ~= true) and (LaunchUpdateURL ~= true)) then
	Button.SetEnabled("retry_button", true);
	change_lang_message ("none","91",ExistingUpdateText,"ok")  
	Button.SetText("retry_button", text_message);
end

--Enable Back Button
Button.SetEnabled("back_button", true);</Script>
