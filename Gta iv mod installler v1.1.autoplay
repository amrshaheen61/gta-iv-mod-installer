	-----RenameDir------------------------------------------------------------------------------------------------------
		
		function Backup_RenameDir ()
			
			--Determine is using game directory or user files directory
			if (ScriptActions_2_Current == "userfiles") then Install_FileRename_DirType = UserFiles; end
			if (ScriptActions_2_Current == "gamedir") then Install_FileRename_DirType = DetectedDirFinal .. "\\"; end
			
			--See if directory exists
			Backup_RenameDir_ExistingFolderExists = Folder.DoesExist(Install_FileRename_DirType .. "\\" ..ScriptActions_3_Current);
			
			if (Backup_RenameDir_ExistingFolderExists == true) then
				--Parse filename
				Backup_RenameDir_BackslashPos = String.ReverseFind(ScriptActions_3_Current, "\\", false);
				if (Backup_RenameDir_BackslashPos == -1) then
					--There is NOT a backslash
					BackupScript_Value = "|RenameDir|" .. ScriptActions_2_Current .. "|" .. ScriptActions_4_Current .. "|" .. ScriptActions_3_Current .. "|abort|";
					BackupScriptInsert();
				else
					--There IS at least one backslash
					--Remove ending backslashes (if there are any)
					RemoveEndSlash_Value = ScriptActions_3_Current;
					RemoveEndSlash();
					ScriptActions_3_Current = RemoveEndSlash_Value;
					--Count string characters
					Backup_RenameDir_StringCount = String.Length(ScriptActions_3_Current);
					Backup_RenameDir_BackslashPos2 = String.ReverseFind(ScriptActions_3_Current, "\\", false);
					Backup_RenameDir_DirNameCount = Backup_RenameDir_StringCount - Backup_RenameDir_BackslashPos2;
					--Get info after the last backslash (the name of the folder to be renamed)
					Backup_RenameDir_DirNameOnly = String.Right(ScriptActions_3_Current, Backup_RenameDir_DirNameCount);
					--Get info before final backslash (subfolders names only)
					BackupRenameDir_SubfolderCount = Backup_RenameDir_StringCount - Backup_RenameDir_DirNameCount;
					Backup_RenameDir_SubfolderOnly = String.Left(ScriptActions_3_Current, BackupRenameDir_SubfolderCount);
					--Insert backup command into script
					BackupScript_Value = "|RenameDir|" .. ScriptActions_2_Current .. "|" .. Backup_RenameDir_SubfolderOnly .. "\\" .. ScriptActions_4_Current .. "|" .. Backup_RenameDir_DirNameOnly .. "|abort|";
					BackupScriptInsert();
				end
			end
		end
		
	
	
	
	
	
	
	
