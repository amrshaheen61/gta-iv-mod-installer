<Script>--Set Mod Info For First Script

Paragraph.SetText("scripts_ModName&amp;ModVer", ModName .. "  v" .. ModVer);
change_lang_message("page_Script_Selection_texts","15",ModExtraInfo,"ok")
Paragraph.SetText("scripts_ModExtraInfo", text_message);
change_lang_message("page_Script_Selection_texts","16",ModDesc,"ok")
Paragraph.SetText("scripts_ModDesc", text_message);
--Empty ListBox
ListBox.DeleteItem("ScriptList", -1);

--Assign Variables
ScriptsCurrentNum = 0;

while (ScriptsCurrentNum &lt; ScriptsFoundCount) do --If there's still more scripts in the table...
	--Move to next script in the table
	ScriptsCurrentNum = ScriptsCurrentNum + 1;
	--Assign Current Script to Variable
	CurrentScriptFullPath = ScriptsFound[ScriptsCurrentNum];
	

	
	--Get Script Name Only ("script.sami")
	CurrentScriptPathTable = String.SplitPath(CurrentScriptFullPath);
	--Merge Filename and Extension Together
	CurrentScriptName = String.Concat(CurrentScriptPathTable.Filename, CurrentScriptPathTable.Extension);
	--Add Current Script to ListBox
	ListBoxAdd = ListBox.AddItem("ScriptList", CurrentScriptName, CurrentScriptName);
end

--Specify which script should be selected
ListBox.SelectItem("ScriptList", CurrentScriptSelected);


   
change_lang ("Paragraph1","19","Paragraph");

change_lang ("scripts_ModName&amp;ModVer","20","Paragraph_no");
change_lang ("scripts_ModDesc","21","Paragraph_no");
change_lang ("scripts_ModExtraInfo","22","Paragraph_no");

change_lang ("Button2","23","Button");
change_lang ("Button1","24","Button");
</Script>
