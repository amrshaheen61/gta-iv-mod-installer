function DisectCommand ()
	--Input Variables: "DC_CurrentScriptCommand"   |    Return Variables: table "DC_ActionTable"
	
	--Assign Variables
	DC_ScriptAction_1 = "";
	DC_ScriptAction_2 = "";
	DC_ScriptAction_3 = "";
	DC_ScriptAction_4 = "";
	DC_ScriptAction_5 = "";
	DC_ActionTable = {};
	CancelActions = false;
	DC_CurrentCommandTableColumn = 0;
	DC_CurrentSEPPos = 0;
	DC_CurrentCmdAction = nil;
	
	--Count Number of Characters in the CurrentScriptCommand
	DC_CurrentScriptLength = String.Length(DC_CurrentScriptCommand);
	if (DC_CurrentScriptLength == -1) then
		CancelActions = true;
change_lang_message("page_Scripter{2}_texts","139","none","ok")
		Error103s = Dialog.Message(text_title, text_message, MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);					
	else
		--Create Empty Table to Hold Command Actions
		DC_CurrentCommandTable = {};
		while ((DC_CurrentSEPPos &lt; DC_CurrentScriptLength) and (DC_CurrentCmdAction ~= "") and (CancelActions ~= true)) do
			---Get Next Command Action (i.e. "ImgAdd" or "police.dff" and so on)---
			--Reset Variables
			DC_CurrentStartSEP = "";
			DC_CurrentEndSEP = "";
			--Find First | (SEP)
			DC_CurrentSEPPos = DC_CurrentSEPPos + 1
			DC_CurrentStartSEP = String.Find(DC_CurrentScriptCommand, "|", DC_CurrentSEPPos, false);
			if (DC_CurrentStartSEP ~= -1) then --If the first SEParator is found...
				if (DC_CurrentStartSEP &lt; DC_CurrentScriptLength) then
					--Find Second | (SEP)
					DC_CurrentSEPPos = DC_CurrentStartSEP + 1;
					DC_CurrentStopSEP = String.Find(DC_CurrentScriptCommand, "|", DC_CurrentSEPPos, false);
					if (DC_CurrentStopSEP == -1) then
						--Do nothing with current line. Move to next line
					else
						--Take off the | (SEPs) from command action
						DC_CurrentStartSEP = DC_CurrentStartSEP + 1;
						DC_CurrentStopSEP = DC_CurrentStopSEP - 1;
						--Find out the length of the current command action
						DC_CurrentSEPLength = DC_CurrentStopSEP - DC_CurrentStartSEP + 1;
						--Get the command action into a variable
						DC_CurrentCmdAction = String.Mid(DC_CurrentScriptCommand, DC_CurrentStartSEP, DC_CurrentSEPLength);
						--Add the current action to the table
						DC_CurrentCommandTableColumn = DC_CurrentCommandTableColumn + 1;
						Table.Insert(DC_ActionTable, DC_CurrentCommandTableColumn, DC_CurrentCmdAction);
					end
				end
			end
		end
	end
	--Clear Input Variables
	DC_CurrentScriptCommand = "";
end



