	-----DeleteText------------------------------------------------------------------------------------------------------
		
		function Backup_DeleteText ()
			
			--Find out which line number the existing line is
			Backup_DeleteText_ReadFile = TextFile.ReadToTable(DetectedDirFinal .. "\\" .. ScriptActions_3_Current);
			if (Backup_DeleteText_ReadFile == nil) then
				--The file could NOT be read
				CancelActions = true;
change_lang_message("Global_functions_texts","336",DetectedDirFinal.."\\"..ScriptActions_3_Current,"ok")
				Error504 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
				--Log Error
change_lang_message("Global_functions_texts","337",DetectedDirFinal.."\\"..ScriptActions_3_Current,"ok")
				ErrorLog_Value = text_message;
				ErrorLogUpdate();
				RemoveBackupContents = true;
			else
				--The file WAS read
				--Get line number for specified line
				Backup_DeleteText_FileContents = TextFile.ReadToTable(DetectedDirFinal .. "\\" .. ScriptActions_3_Current);
				--Count number of lines
				Backup_DeleteText_LineCount = Table.Count(Backup_DeleteText_FileContents);
				--Search for specified line
				Backup_DeleteText_FileCounter = 0;
				StopLoop = false;
				Backup_DeleteText_WasFound = false;
				while ((Backup_DeleteText_FileCounter &lt; Backup_DeleteText_LineCount) and (StopLoop ~= true)) do
					Backup_DeleteText_FileCounter = Backup_DeleteText_FileCounter + 1;
					Backup_DeleteText_CurrentLine = Backup_DeleteText_FileContents[Backup_DeleteText_FileCounter];
					--See if current line the the correct one
					Backup_DeleteText_IsCurrentLine = String.Find(Backup_DeleteText_CurrentLine, ScriptActions_2_Current, 1, false);
					if (Backup_DeleteText_IsCurrentLine ~= -1) then
						StopLoop = true;
						if (Backup_DeleteText_FileCounter == 1) then
							--The specified line IS the first one in the file
							BackupScript_Value = "|InsertText|" .. Backup_DeleteText_CurrentLine .. "|" .. ScriptActions_3_Current .. "|start|nil|";
							BackupScriptInsert();
						else
							--The specified line is NOT the first one in the file
							--Get the line located before it
							StopLoop2 = false;
							Backup_DeleteText_FileCounter2 = Backup_DeleteText_FileCounter;
							while ((StopLoop2 ~= true) and (Backup_DeleteText_FileCounter2 ~= 0) and (Backup_DeleteText_FileCounter2 ~= -1)) do
								Backup_DeleteText_FileCounter2 = Backup_DeleteText_FileCounter2 - 1;
								if (Backup_DeleteText_FileCounter2 == 1) then
									--Put the line at the start of the file
									StopLoop2 = true;
									BackupScript_Value = "|InsertText|" .. Backup_DeleteText_CurrentLine .. "|" .. ScriptActions_3_Current .. "|start|nil|";
									BackupScriptInsert();
								else
									Backup_DeleteText_CurrentLine2 = Backup_DeleteText_FileContents[Backup_DeleteText_FileCounter2];
									--See if the current line is not blank
									if (Backup_DeleteText_CurrentLine2 == "") then
										--Don't use this line because it's blank
									else
										--See if the line has only one character
										Backup_DeleteText_CurrentLine_Count = String.Length(Backup_DeleteText_CurrentLine2);
										if (Backup_DeleteText_CurrentLine_Count == 1) then
											--Don't use this line
										else
											--Use the current line as the search line
											StopLoop2 = true;
											BackupScript_Value = "|InsertText|" .. Backup_DeleteText_CurrentLine .. "|" .. ScriptActions_3_Current .. "|below|" .. Backup_DeleteText_CurrentLine2 .. "|";
											BackupScriptInsert();
										end
									end
								end
							end
						end
					end
				end
			end
		end
		
		
		
