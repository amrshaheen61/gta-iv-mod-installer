<Event>
<Name>On Click</Name>
<Args/>
<Script>--Assign Default Variables
CancelActions = false;
VerifyLineHAN_Table = {};
VerifyLineIDE_Table = {};
VerifyLineCOL_Table = {};

--Get selected HAN line
Vehicle_AssinLineHAN_Selected = ListBox.GetSelected("handling_lines");
if (Vehicle_AssinLineHAN_Selected == nil) then
change_lang_message("page_Vehicle_Info_texts","36","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
	CancelActions = true;
else
	--Get data for selected item
	Vehicle_AssinLineHAN_SelectedData = ListBox.GetItemData("handling_lines", Vehicle_AssinLineHAN_Selected[1]);
	if (Vehicle_AssinLineHAN_SelectedData == "") then
	
change_lang_message("page_Vehicle_Info_texts","37","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
		CancelActions = true;
	else
		VerifyLineHAN_Table[1] = Vehicle_AssinLineHAN_SelectedData;
		--Verify handling line
		VerifyLineHAN_Returned = VerifyLineHAN(VerifyLineHAN_Table,VehicleIDE_Handling);
		if (VerifyLineHAN_Returned[1] == nil) then
			--The handling line is invalid
			CancelActions = true;
			
change_lang_message("page_Vehicle_Info_texts","38","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
		else
			--Remove Tabs and DoubleSpaces
			VerifyLineHAN_Returned_Cleaned = RemoveTabDoubleSpace(VerifyLineHAN_Returned[1]);
			--Delimit the chosen handling line
			VehicleLineHAN_Delimited_Return = Delimit(VerifyLineHAN_Returned_Cleaned, " ", false);
			if (VehicleLineHAN_Delimited_Return == nil) then
				--Error delimiting handline line
				CancelActions = true;
				
change_lang_message("page_Vehicle_Info_texts","39","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
				else
				Vehicle_SelectedLine_HAN = VerifyLineHAN_Returned[1];
				Vehicle_SelectedLine_HAN_Delimited = VehicleLineHAN_Delimited_Return;
			end
		end
	end
end


--Get selected IDE line
Vehicle_AssinLineIDE_Selected = ListBox.GetSelected("ide_lines");
if (Vehicle_AssinLineIDE_Selected == nil) then

change_lang_message("page_Vehicle_Info_texts","40","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
	CancelActions = true;
else
	--Get data for selected item
	Vehicle_AssinLineIDE_SelectedData = ListBox.GetItemData("ide_lines", Vehicle_AssinLineIDE_Selected[1]);
	if (Vehicle_AssinLineIDE_SelectedData == "") then
change_lang_message("page_Vehicle_Info_texts","41","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
		CancelActions = true;
	else
		VerifyLineIDE_Table[1] = Vehicle_AssinLineIDE_SelectedData;
		--Verify IDE line
		VerifyLineIDE_Returned = VerifyLineIDE(VerifyLineIDE_Table,VehicleIDE_Handling);
		if (VerifyLineIDE_Returned[1] == nil) then
			--The line is invalid
			CancelActions = true;
change_lang_message("page_Vehicle_Info_texts","42","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
		else
			--Remove Tabs and Spaces
			VerifyLineIDE_Returned_Cleaned = RemoveTabSpace(VerifyLineIDE_Returned[1]);
			--Delimit the chosen IDE line
			VehicleLineIDE_Delimited_Return = Delimit(VerifyLineIDE_Returned_Cleaned, ",", false);
			if (VehicleLineIDE_Delimited_Return == nil) then
				--Error delimiting line
				CancelActions = true;
change_lang_message("page_Vehicle_Info_texts","43","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
			else
				Vehicle_SelectedLine_IDE = VerifyLineIDE_Returned[1];
				Vehicle_SelectedLine_IDE_Delimited = VehicleLineIDE_Delimited_Return;
			end
		end
	end
end


--Get selected COL line
if (VehicleCOL_Original_HasColLineCheck == false) then
	Vehicle_AssinLineCOL_Selected = ListBox.GetSelected("carcols_lines");
	if (Vehicle_AssinLineCOL_Selected == nil) then
	
change_lang_message("page_Vehicle_Info_texts","44","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
		CancelActions = true;
	else
		--Get data for selected item
		Vehicle_AssinLineCOL_SelectedData = ListBox.GetItemData("carcols_lines", Vehicle_AssinLineCOL_Selected[1]);
		if (Vehicle_AssinLineCOL_SelectedData == "") then
change_lang_message("page_Vehicle_Info_texts","45","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
			CancelActions = true;
		else
			VerifyLineCOL_Table[1] = Vehicle_AssinLineCOL_SelectedData;
			--Verify COL line
			VerifyLineCOL_Returned = VerifyLineCOL(VerifyLineCOL_Table,VehicleCOL_Handling);
			if (VerifyLineCOL_Returned[1] == nil) then
				--The line is invalid
				CancelActions = true;
				
change_lang_message("page_Vehicle_Info_texts","46","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
			else
				--Remove Tabs and Spaces
				VerifyLineCOL_Returned_Cleaned = RemoveTabDoubleSpace(VerifyLineCOL_Returned[1]);
				--Delimit the chosen COL line
				VehicleLineCOL_Delimited_Return = Delimit(VerifyLineCOL_Returned_Cleaned, " ", false);
				if (VehicleLineCOL_Delimited_Return == nil) then
					--Error delimiting line
					CancelActions = true;
					
change_lang_message("page_Vehicle_Info_texts","47","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
				else
					Vehicle_SelectedLine_COL = VerifyLineCOL_Returned[1];
					Vehicle_SelectedLine_COL_Delimited = VehicleLineCOL_Delimited_Return;
				end
			end
		end
	end
end



--Go to next page if there were no errors
if (CancelActions ~= true) then
	--Go to next page
	Page.Jump("Vehicle Selection");
end</Script>
