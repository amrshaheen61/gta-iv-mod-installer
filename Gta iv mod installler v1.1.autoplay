			--Add a blank entry
			resultdffd = ListBox.AddItem("commandlist", " ", "");
			Scripter_Arrange_BlankEntry = true;
		end
		resultfff = ListBox.InsertItem("commandlist", Scripter_Arrange_InsertPos, Scripter_Arrange_SelectedText, Scripter_Arrange_SelectedData);
