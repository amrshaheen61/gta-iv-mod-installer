function SearchTxt (SearchTxt_String,SearchTxt_Files_Table)
	--Assign Variables
	SearchTxt_TableCount = 0;
	SearchTxt_ReturnLine = {};
	SearchTxt_ReturnPos = {};
	SearchTxt_ReturnFile = {};
	SearchTxt_Files_Counter = 0;
	
	--Count # of Specified Text Files
	SearchTxt_Files_Table_Count = Table.Count(SearchTxt_Files_Table);
	if (SearchTxt_Files_Table ~= 0) then
		while (SearchTxt_Files_Counter &lt; SearchTxt_Files_Table_Count) do
			SearchTxt_Files_Counter = SearchTxt_Files_Counter + 1;
			--Get Next Specified File
			SearchTxt_CurrentFile = SearchTxt_Files_Table[SearchTxt_Files_Counter];	
			--Read File to Table
			SearchTxt_Table = TextFile.ReadToTable(SearchTxt_CurrentFile);
			if (SearchTxt_Table ~= nil) then
				--Count table lines
				SearchTxt_TableCount = Table.Count(SearchTxt_Table);
				SearchTxt_Counter = 0;
				while (SearchTxt_Counter &lt; SearchTxt_TableCount) do
					SearchTxt_Counter = SearchTxt_Counter + 1;
					--Get next line in table
					SearchTxt_TableLine = SearchTxt_Table[SearchTxt_Counter];
					--Search Line and See if specified value exists
					SearchTxt_SearchResult = String.Find(SearchTxt_TableLine, SearchTxt_String, 1, true);
					if (SearchTxt_SearchResult ~= -1) then  --Value IS found
						--Assign Position
						SearchTxt_ReturnPos_Count = Table.Count(SearchTxt_ReturnPos);
						SearchTxt_ReturnPos_InsertPos = SearchTxt_ReturnPos_Count + 1;
						SearchTxt_ReturnPos[SearchTxt_ReturnPos_InsertPos] = SearchTxt_Counter;
						--Assign Value
						SearchTxt_ReturnLine_Count = Table.Count(SearchTxt_ReturnLine);
						SearchTxt_ReturnLine_InsertPos = SearchTxt_ReturnLine_Count + 1;
						SearchTxt_ReturnLine[SearchTxt_ReturnLine_InsertPos] = SearchTxt_TableLine;
						--Assign Filename
						SearchTxt_ReturnFile_Count = Table.Count(SearchTxt_ReturnFile);
						SearchTxt_ReturnFile_InsertPos = SearchTxt_ReturnFile_Count + 1;
						SearchTxt_ReturnFile[SearchTxt_ReturnFile_InsertPos] = SearchTxt_CurrentFile;
					end
				end
			end
		end
	end
	--Return Values
	return SearchTxt_ReturnPos, SearchTxt_ReturnLine, SearchTxt_ReturnFile;
end





--Get Specified Vehicle "Column" From All Vehicles
function GetVehicleColumn (ColumnNum)

	local VehicleData_Counter = 0;
	VehicleData_CurrentLineDelimited = {};
	VehicleColumn = {};
	
	---Count VehicleData Table
	VehicleData_Count = Table.Count(VehicleData);
	while (VehicleData_Counter &lt; VehicleData_Count) do
		VehicleData_Counter = VehicleData_Counter + 1;
		
		--Get next vehicle name
		VehicleData_CurrentLine = VehicleData[VehicleData_Counter];
		VehicleData_CurrentLineDelimited = Delimit(VehicleData_CurrentLine, ",", true);
		if (VehicleData_CurrentLineDelimited ~= nil) then
			VehicleColumn_Count = Table.Count(VehicleColumn);
			VehicleColumn_Counter = VehicleColumn_Count + 1;
			VehicleColumn[VehicleColumn_Counter] = VehicleData_CurrentLineDelimited[ColumnNum];
		end
	end
	--Return Value
	return VehicleColumn;
end

