function Scripter_ListAdd ()
	--Assign Variables
	Scripter_ListAdd_Counter = 0;
	
	if (CancelActions ~= true) then
		--Count how many commands will be added to the list
		Scripter_Command_Count = Table.Count(ScriptActions_1);
		
		--Clear Existing data from list box
		ListBox.DeleteItem("commandlist", -1);
		
		while ((Scripter_ListAdd_Counter &lt; Scripter_Command_Count) and (CancelActions ~= true)) do
			Scripter_ListAdd_Counter = Scripter_ListAdd_Counter + 1;
			
			--Figure out how many actions in current command, then create complete command string
			Scripter_ListAdd_Current = "|" .. ScriptActions_1[Scripter_ListAdd_Counter] .. "|" .. ScriptActions_2[Scripter_ListAdd_Counter] .. "|";
			if (ScriptActions_3[Scripter_ListAdd_Counter] ~= "nil") then
				Scripter_ListAdd_Current = "|" .. ScriptActions_1[Scripter_ListAdd_Counter] .. "|" .. ScriptActions_2[Scripter_ListAdd_Counter] .. "|" .. ScriptActions_3[Scripter_ListAdd_Counter] .. "|";
			end
			if (ScriptActions_4[Scripter_ListAdd_Counter] ~= "nil") then
				Scripter_ListAdd_Current = "|" .. ScriptActions_1[Scripter_ListAdd_Counter] .. "|" .. ScriptActions_2[Scripter_ListAdd_Counter] .. "|" .. ScriptActions_3[Scripter_ListAdd_Counter] .. "|" .. ScriptActions_4[Scripter_ListAdd_Counter] .. "|";
			end
			if (ScriptActions_5[Scripter_ListAdd_Counter] ~= "nil") then
				Scripter_ListAdd_Current = "|" .. ScriptActions_1[Scripter_ListAdd_Counter] .. "|" .. ScriptActions_2[Scripter_ListAdd_Counter] .. "|" .. ScriptActions_3[Scripter_ListAdd_Counter] .. "|" .. ScriptActions_4[Scripter_ListAdd_Counter] .. "|" .. ScriptActions_5[Scripter_ListAdd_Counter] .. "|";
			end
			
			--Add current script data to listbox
			Scripter_ListAdd_ListPos = ListBox.AddItem("commandlist", Scripter_ListAdd_Current, ScriptActions_1[Scripter_ListAdd_Counter]);
		end
	end
end
</Script>
