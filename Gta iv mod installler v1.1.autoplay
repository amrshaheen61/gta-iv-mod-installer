function VerifyLineIDE (VerifyLineIDE_Value_Table,VerifyLineIDE_HandlingName_String)
	--Assign Variables
	VerifyLineIDE_Return = {}; --Returns a table containing all verified lines
	VerifyLineIDE_Counter = 0;
	
	--Count How Many IDE Lines Were Specified
	VerifyLineIDE_Value_Table_Count = Table.Count(VerifyLineIDE_Value_Table);
	while (VerifyLineIDE_Counter &lt; VerifyLineIDE_Value_Table_Count) do
		VerifyLineIDE_Counter = VerifyLineIDE_Counter + 1;
		--Get Next Line That Is to Be Verified
		VerifyLineIDE_CurrentLineFull = VerifyLineIDE_Value_Table[VerifyLineIDE_Counter];
		

		-------------------------Run All Verification Proceedures-----------------------------
		
		--See Where the IDE Number Is In Current Line
		VerifyLineIDE_NumPos = String.Find(VerifyLineIDE_CurrentLineFull, VehicleIDE_Number, 1, true);
		VerifyLineIDE_CurrentLineFull_Length = String.Length(VerifyLineIDE_CurrentLineFull);

		if (VerifyLineIDE_NumPos ~= -1) then
			--Get New Line and Remove Info to Left of IDE Number
			VerifyLineIDE_NameCharsToGet = VerifyLineIDE_CurrentLineFull_Length - VerifyLineIDE_NumPos + 1;
			VerifyLineIDE_CurrentLine_New = String.Right(VerifyLineIDE_CurrentLineFull, VerifyLineIDE_NameCharsToGet);
			--Delimit Line
			VerifyLineIDE_CurrentLine_Table = Delimit(VerifyLineIDE_CurrentLine_New, ",", true);
			--Make Sure IDE Number Has No Extra Characters
			VerifyLineIDE_ExtraCharCheck = VerifyLineIDE_CurrentLine_Table[1];
			if (VerifyLineIDE_ExtraCharCheck ~= VehicleIDE_Number) then
				--IDE Number Has Extra Characters
			else
				--Count Fields In Current Line
				VerifyLineIDE_CurrentLine_Table_Count = Table.Count(VerifyLineIDE_CurrentLine_Table);
				
				--Count Fields In ORIGINAL IDE Line
				VerifyLineIDE_Original_FieldCount = Table.Count(VehicleIDE_Original_Delim);
				
				if (VerifyLineIDE_CurrentLine_Table_Count ~= VerifyLineIDE_Original_FieldCount) then
					--The Line Is Invalid (Not Enough Fields As Original)
				else
					--Assemble Final IDE Line
					VerifyLineIDE_FinalLine = "";
					VerifyLineIDE_Assemble_Counter = 0;
					while (VerifyLineIDE_Assemble_Counter &lt; VerifyLineIDE_CurrentLine_Table_Count) do
						VerifyLineIDE_Assemble_Counter = VerifyLineIDE_Assemble_Counter + 1;
						--Get Next "Piece" of Ide Data
						VerifyLineIDE_Assemble_CurrentPiece = VerifyLineIDE_CurrentLine_Table[VerifyLineIDE_Assemble_Counter];
						if (VerifyLineIDE_Assemble_Counter == VerifyLineIDE_CurrentLine_Table_Count) then
							--If working with last value, don't add the comma\space\tab
							--Add current piece to the final line
							VerifyLineIDE_FinalLine = VerifyLineIDE_FinalLine .. VerifyLineIDE_Assemble_CurrentPiece;							
						else
							--Add current piece to the final line
							VerifyLineIDE_FinalLine = VerifyLineIDE_FinalLine .. VerifyLineIDE_Assemble_CurrentPiece .. ", \t";
						end
					end
					
					if (VerifyLineIDE_FinalLine ~= "") then
						--Add Current Line to Return Table
						VerifyLineIDE_Return_Count = Table.Count(VerifyLineIDE_Return);
						VerifyLineIDE_Return_InsertPos = VerifyLineIDE_Return_Count + 1;
						VerifyLineIDE_Return[VerifyLineIDE_Return_InsertPos] = VerifyLineIDE_FinalLine;
					end
				end
			end
		end
	end
	--Return Table Containing All Valid IDE Lines Only
	return VerifyLineIDE_Return;
end


