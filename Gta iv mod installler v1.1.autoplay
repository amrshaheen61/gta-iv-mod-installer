<Event>
<Name>On Show</Name>
<Args/>
<Script>change_lang ("Paragraph1","31","Paragraph");
change_lang ("Paragraph2","32","Paragraph");
change_lang ("Paragraph3","33","Paragraph");

change_lang ("handlingcfg_heading","he","Paragraph_no");
change_lang ("vehicleside_heading","he","Paragraph_no");
change_lang ("carcolsdat_heading","he","Paragraph_no");

change_lang ("continue_button","34","Button");
change_lang ("back_vehicle-page","35","Button");

--Remove Any Existing Items From Listbox
ListBox.DeleteItem("handling_lines", -1);
ListBox.DeleteItem("ide_lines", -1);
ListBox.DeleteItem("carcols_lines", -1);


--See if there are multiple handling lines
VehicleHAN_Lines_Count = Table.Count(VehicleHAN_Lines);
if (VehicleHAN_Lines_Count == 0) then
	--Add Default Handling Line to Text Box
	Vehicle_MultiHAN_AddedLocation = ListBox.AddItem("handling_lines", VehicleHAN_Original_Cleaned, VehicleHAN_Original_Cleaned);
change_lang_message("page_Vehicle_Info_texts","23","none","none")
	Paragraph.SetText("handlingcfg_heading", text_message);
	
else
	if (VehicleHAN_Lines_Count &gt; 1) then
change_lang_message("page_Vehicle_Info_texts","24","none","none")
		Paragraph.SetText("handlingcfg_heading", text_message);
	
	else
change_lang_message("page_Vehicle_Info_texts","25","none","none")
		Paragraph.SetText("handlingcfg_heading", text_message);
		
	end
	Vehicle_MultiHAN_Counter = 0;
	while (Vehicle_MultiHAN_Counter &lt; VehicleHAN_Lines_Count) do
		Vehicle_MultiHAN_Counter = Vehicle_MultiHAN_Counter + 1;
		--Get Next Handling Line
		Vehicle_MultiHAN_CurrentLine = VehicleHAN_Lines[Vehicle_MultiHAN_Counter];
		--See if line already exists in list box
		Vehicle_MultiHAN_ExistingSearch = ListBox.FindItem("handling_lines", -1, LB_BYTEXT, Vehicle_MultiHAN_CurrentLine);
		if (Vehicle_MultiHAN_ExistingSearch == -1) then
			--Add Current Line to Text Box
			Vehicle_MultiHAN_AddedLocation = ListBox.AddItem("handling_lines", Vehicle_MultiHAN_CurrentLine, Vehicle_MultiHAN_CurrentLine);
		end
	end
end
--Make First Item In the List the Default Selected
ListBox.SelectItem("handling_lines", 1);





--See if there are multiple IDE lines
VehicleIDE_Lines_Count = Table.Count(VehicleIDE_Lines);
if (VehicleIDE_Lines_Count == 0) then
	--Add Default IDE Line to Text Box
	Vehicle_MultiIDE_AddedLocation = ListBox.AddItem("ide_lines", VehicleIDE_Original_Cleaned, VehicleIDE_Original_Cleaned);
change_lang_message("page_Vehicle_Info_texts","26","none","none")
		
	Paragraph.SetText("vehicleside_heading", text_message);

else
	if (VehicleIDE_Lines_Count &gt; 1) then
change_lang_message("page_Vehicle_Info_texts","27","none","none")
		Paragraph.SetText("vehicleside_heading", text_message);
	
	else
change_lang_message("page_Vehicle_Info_texts","28","none","none")
		Paragraph.SetText("vehicleside_heading", text_message);
	
	end
	Vehicle_MultiIDE_Counter = 0;
	while (Vehicle_MultiIDE_Counter &lt; VehicleIDE_Lines_Count) do
		Vehicle_MultiIDE_Counter = Vehicle_MultiIDE_Counter + 1;
		--Get Next IDE Line
		Vehicle_MultiIDE_CurrentLine = VehicleIDE_Lines[Vehicle_MultiIDE_Counter];
		--See if line already exists in list box
		Vehicle_MultiIDE_ExistingSearch = ListBox.FindItem("ide_lines", -1, LB_BYTEXT, Vehicle_MultiIDE_CurrentLine);
		if (Vehicle_MultiIDE_ExistingSearch == -1) then
			--Add Current Line to Text Box
			Vehicle_MultiIDE_AddedLocation = ListBox.AddItem("ide_lines", Vehicle_MultiIDE_CurrentLine, Vehicle_MultiIDE_CurrentLine);
		end
	end
end
--Make First Item In the List the Default Selected
ListBox.SelectItem("ide_lines", 1);




--See if there are multiple CARCOLS lines
if (VehicleCOL_Original_HasColLineCheck == true) then
	--If vehicle has no carcols line...
	ListBox.DeleteItem("carcols_lines", -1);
change_lang_message("page_Vehicle_Info_texts","29","none","none")
	Paragraph.SetText("carcolsdat_heading", text_message);

else
	VehicleCOL_Lines_Count = Table.Count(VehicleCOL_Lines);
	if (VehicleCOL_Lines_Count == 0) then
		--Add Default COL Line to Text Box
		Vehicle_MultiCOL_AddedLocation = ListBox.AddItem("carcols_lines", VehicleCOL_Original_Cleaned, VehicleCOL_Original_Cleaned);
change_lang_message("page_Vehicle_Info_texts","30","none","none")
		Paragraph.SetText("carcolsdat_heading", text_message);
	
	else
		if (VehicleCOL_Lines_Count &gt; 1) then
change_lang_message("page_Vehicle_Info_texts","31","none","none")
			Paragraph.SetText("carcolsdat_heading", text_message);
			
		else
change_lang_message("page_Vehicle_Info_texts","32","none","none")
			Paragraph.SetText("carcolsdat_heading", text_message);
		
		end
		Vehicle_MultiCOL_Counter = 0;
		while (Vehicle_MultiCOL_Counter &lt; VehicleCOL_Lines_Count) do
			Vehicle_MultiCOL_Counter = Vehicle_MultiCOL_Counter + 1;
			--Get Next COL Line
			Vehicle_MultiCOL_CurrentLine = VehicleCOL_Lines[Vehicle_MultiCOL_Counter];
			--See if line already exists in list box
			Vehicle_MultiCOL_ExistingSearch = ListBox.FindItem("carcols_lines", -1, LB_BYTEXT, Vehicle_MultiCOL_CurrentLine);
			if (Vehicle_MultiCOL_ExistingSearch == -1) then
				--Add Current Line to Text Box
				Vehicle_MultiCOL_AddedLocation = ListBox.AddItem("carcols_lines", Vehicle_MultiCOL_CurrentLine, Vehicle_MultiCOL_CurrentLine);
			end
		end
	end
	--Make First Item In the List the Default Selected
	ListBox.SelectItem("carcols_lines", 1);
end</Script>
