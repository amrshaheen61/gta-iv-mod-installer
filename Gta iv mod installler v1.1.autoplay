<Script>--Assign Variables
CancelActions = false;
--Disable Install Button
Button.SetEnabled("continue_button", false);
change_lang_message("page_Vehicle_Selection_texts","48","none","none")
Button.SetText("continue_button", text_message);

--Make sure user has entered something into the vehicle name box
VehicleName_Text = Input.GetText("vehicle_name");
if (VehicleName_Text == "") then
change_lang_message("page_Vehicle_Selection_texts","49","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
else
	--Make sure the name doesn't have invalid characters
	InvalidCharDetect_String = VehicleName_Text;
	InvalidCharDetect();
	if (InvalidCharFound == true) then
change_lang_message("page_Vehicle_Selection_texts","50","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
		else
		--Make sure a vehicle is selected
		VehicleInstall_SelectedItems = ListBox.GetSelected("vehicle_list");
		if (VehicleInstall_SelectedItems == nil) then
			--Nothing is selected
			CancelActions = true;
change_lang_message("page_Vehicle_Selection_texts","51","none","ok")
	result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
		else
			VehicleInstall_SelectedPos = VehicleInstall_SelectedItems[1];
			--Get text\data for currently selected vehicle
			VehicleInstall_SelectedData = ListBox.GetItemData("vehicle_list", VehicleInstall_SelectedPos);
			VehicleInstall_SelectedText = ListBox.GetItemText("vehicle_list", VehicleInstall_SelectedPos);
			if ((VehicleInstall_SelectedData == "") or (VehicleInstall_SelectedText == "")) then
				--Error getting text or data
				CancelActions = true;
change_lang_message("page_Vehicle_Selection_texts","52","none","ok")
				Error270 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
				--Log Error
				ErrorLog_Value = "ERROR 270 - "..text_message.."";
				ErrorLogUpdate();
				ExitGIMI();
			else
			
				--Get original IDE line for selected vehicle--------------------------------------------------------------
				Selected_OriginalLineIDEPosReturn, Selected_OriginalLineIDEReturn = SearchTable(VehicleInstall_SelectedData,VehicleList);
				Selected_OriginalLineIDEPos = Selected_OriginalLineIDEPosReturn[1];
				Selected_OriginalLineIDE = VehicleData[Selected_OriginalLineIDEPos];
				if (Selected_OriginalLineIDE == nil) then
					CancelActions = true;
change_lang_message("page_Vehicle_Selection_texts","53","none","ok")
					Error271 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
					--Log Error
					ErrorLog_Value = "ERROR 271 - "..text_message.."";
					ErrorLogUpdate();
					ExitGIMI();
				end
				--Remove Tabs and Doublespaces
				Selected_OriginalLineIDE_Cleaned = RemoveTabDoubleSpace(Selected_OriginalLineIDE);
				--Delimit IDE Line to A Table
				Selected_OriginalLineIDE_Delim = Delimit(Selected_OriginalLineIDE_Cleaned, ",", false)
				if (Selected_OriginalLineIDE_Delim == nil) then
					CancelActions = true;
change_lang_message("page_Vehicle_Selection_texts","53","none","ok")
					Error271 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
					--Log Error
					ErrorLog_Value = "ERROR 271 - "..text_message.."";
					ErrorLogUpdate();
					ExitGIMI();
				end
				--------------------------------------------------------------------------------------------------
				--Break IDE Line Into a Table With Each Value
				SelectedIDE_Table = Delimit(Selected_OriginalLineIDE, ",", true);
				
				--Count # of Fields In IDE Line
				SelectedIDE_FieldCount = Table.Count(SelectedIDE_Table);
				
				--Get Vehicle Data File Values
				SelectedIDE_Number = SelectedIDE_Table[1];
				SelectedIDE_Model = SelectedIDE_Table[2];
				SelectedIDE_wtd = SelectedIDE_Table[3];
				SelectedIDE_Type = SelectedIDE_Table[4];
				SelectedIDE_Handling = SelectedIDE_Table[5];
				SelectedIDE_GameName = SelectedIDE_Table[6];
				SelectedIDE_Anims = SelectedIDE_Table[7];
				SelectedIDE_Class = SelectedIDE_Table[8];
				SelectedIDE_Frq1 = SelectedIDE_Table[9];
				SelectedIDE_Frq2 = SelectedIDE_Table[10];
				SelectedIDE_Frq3 = SelectedIDE_Table[11];
				if ((SelectedIDE_Type == "boat") or (SelectedIDE_Model == "skimmer")) then
					--Do Not Get Last Two Values (they are not used for boats or the skimmer)
				else
					SelectedIDE_Flags1 = SelectedIDE_Table[12];
					SelectedIDE_Flags2 = SelectedIDE_Table[13];
					SelectedIDE_Flags3 = SelectedIDE_Table[14];
					SelectedIDE_CompRules = SelectedIDE_Table[15];
				end
		
				
				--Get Original Handling Line For Selected Vehicle----------------------------------------------------------------------
				Selected_OriginalLineHAN_SearchPos, Selected_OriginalLineHAN_Search = SearchTable(SelectedIDE_Handling,VehicleDataH);
				if (Selected_OriginalLineHAN_Search ~= nil) then
					--Assign Original Line to String Variable
					Selected_OriginalLineHAN = Selected_OriginalLineHAN_Search[1];
					--Remove Tabs and Doublespaces
					Selected_OriginalLineHAN_Cleaned = RemoveTabDoubleSpace(Selected_OriginalLineHAN);
					--Delimi Handling Line to A Table
					Selected_OriginalLineHAN_Delim = Delimit(Selected_OriginalLineHAN_Cleaned, " ", false)
					if (Selected_OriginalLineHAN_Delim == nil) then
						CancelActions = true;
change_lang_message("page_Vehicle_Selection_texts","54","none","ok")
						Error272 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
						--Log Error
						ErrorLog_Value = "ERROR 272 - "..text_message.."";
						ErrorLogUpdate();
						ExitGIMI();
					end
				else
					CancelActions = true;
change_lang_message("page_Vehicle_Selection_texts","54","none","ok")
					Error272 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
					--Log Error
					ErrorLog_Value = "ERROR 272 - "..text_message.."";			
					ErrorLogUpdate();
					ExitGIMI();
				end
				---------------------------------------------------------------------------------------------------
				
				--Get Original Carcols Line For Currently Selected Vehicle------------------------------------------------------------------------
				Selected_OriginalLineCOL_HasColLineCheck = NotInCarcols(SelectedIDE_Model);
				if (Selected_OriginalLineCOL_HasColLineCheck == false) then
					Selected_OriginalLineCOL_SearchPos, Selected_OriginalLineCOL_Search = SearchTable(SelectedIDE_Model,VehicleDataC);
					if (Selected_OriginalLineCOL_Search ~= nil) then
						--Assign Original Line to String Variable
						Selected_OriginalLineCOL = Selected_OriginalLineCOL_Search[1];
						--Remove Tabs and Doublespaces
						Selected_OriginalLineCOL_Cleaned = RemoveTabDoubleSpace(Selected_OriginalLineCOL);
						--Delimi Line to A Table
						Selected_OriginalLineCOL_Delim = Delimit(Selected_OriginalLineCOL_Cleaned, " ", true)
						if (Selected_OriginalLineCOL_Delim == nil) then
							CancelActions = true;
change_lang_message("page_Vehicle_Selection_texts","55","none","ok")
							Error273 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
							--Log Error
							ErrorLog_Value = "ERROR 273 - "..text_message.."";
							ErrorLogUpdate();
							ExitGIMI();
						end
					else
						CancelActions = true;
change_lang_message("page_Vehicle_Selection_texts","55","none","ok")
						Error273 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
						--Log Error
						ErrorLog_Value = "ERROR 273 - "..text_message.."";						ErrorLogUpdate();
						ExitGIMI();
					end
				end
				---------------------------------------------------------------------------------------------------
			end
		end
		
		if (CancelActions ~= true) then
		
			--Copy specified data tables
			if (Vehicle_SelectedLine_COL_Delimited == nil) then
				Vehicle_SelectedLine_COL_Final = Selected_OriginalLineCOL_Delim;
			else
				Vehicle_SelectedLine_COL_Final = Vehicle_SelectedLine_COL_Delimited;
			end
			Vehicle_SelectedLine_HAN_Final = Vehicle_SelectedLine_HAN_Delimited;
			Vehicle_SelectedLine_IDE_Final = Vehicle_SelectedLine_IDE_Delimited;
			
			--Modify Handling table for custom vehicle's info to include the changed values to replace a different vehicle
			Vehicle_SelectedLine_HAN_Final[1] = SelectedIDE_Handling;
			
			if (Selected_OriginalLineCOL_HasColLineCheck == false) then
				--Modify Carcols table for custom vehicle's info to include the changed values to replace a different vehicle
				Vehicle_SelectedLine_COL_Final[1] = SelectedIDE_Model;
			end
			
			--Modify IDE table for custom vehicle's info to include the changed values to replace a different vehicle
			Vehicle_SelectedLine_IDE_Final[1] = SelectedIDE_Number;
			Vehicle_SelectedLine_IDE_Final[2] = SelectedIDE_Model;
			Vehicle_SelectedLine_IDE_Final[3] = SelectedIDE_wtd;
			Vehicle_SelectedLine_IDE_Final[4] = SelectedIDE_Type;
			Vehicle_SelectedLine_IDE_Final[5] = SelectedIDE_Handling;
			Vehicle_SelectedLine_IDE_Final[6] = SelectedIDE_GameName;
			Vehicle_SelectedLine_IDE_Final[7] = SelectedIDE_Anims;
			Vehicle_SelectedLine_IDE_Final[8] = SelectedIDE_Class;
			Vehicle_SelectedLine_IDE_Final[9] = SelectedIDE_Frq1;
			Vehicle_SelectedLine_IDE_Final[10] = SelectedIDE_Frq2;
			Vehicle_SelectedLine_IDE_Final[11] = SelectedIDE_Frq3;
			if ((SelectedIDE_Type == "boat") or (SelectedIDE_Model == "skimmer")) then
				--Remove Last Two Values (they are not used for boats or the skimmer)
				result = Table.Remove(Vehicle_SelectedLine_IDE_Final, 12);
				result = Table.Remove(Vehicle_SelectedLine_IDE_Final, 13);
				result = Table.Remove(Vehicle_SelectedLine_IDE_Final, 14);
				result = Table.Remove(Vehicle_SelectedLine_IDE_Final, 15);
			else
				if (Vehicle_SelectedLine_IDE_Final[12] == nil) then
					Vehicle_SelectedLine_IDE_Final[12] = SelectedIDE_Flags1;
				end
				if (Vehicle_SelectedLine_IDE_Final[13] == nil) then
					Vehicle_SelectedLine_IDE_Final[13] = SelectedIDE_Flags2;
				end
				if (Vehicle_SelectedLine_IDE_Final[13] == nil) then
					Vehicle_SelectedLine_IDE_Final[14] = SelectedIDE_Flags3;
				end
				Vehicle_SelectedLine_IDE_Final[15] = SelectedIDE_CompRules;
			end
			
			
			
			--Assemble Final Handling Line
			SelectedHAN_FinalLine = "";
			SelectedHAN_Assemble_Counter = 0;
			--Count items in hadling table
			SelectedHAN_Final_Count = Table.Count(Vehicle_SelectedLine_HAN_Final);
			while (SelectedHAN_Assemble_Counter &lt; SelectedHAN_Final_Count) do
				SelectedHAN_Assemble_Counter = SelectedHAN_Assemble_Counter + 1;
				--Get Next "Piece" of Handling Data
				SelectedHAN_Assemble_CurrentPiece = Vehicle_SelectedLine_HAN_Final[SelectedHAN_Assemble_Counter];
				if (SelectedHAN_Assemble_Counter == 1) then
					--Insert Some Spaces After Handling Name (i.e. "CHEETAH     ")
					SelectedHAN_FinalLine = SelectedHAN_Assemble_CurrentPiece .. "     ";
				else
					--Add current piece to the final line
					SelectedHAN_FinalLine = SelectedHAN_FinalLine .. " " .. SelectedHAN_Assemble_CurrentPiece;
				end
			end
			
			--Assemble Final IDE Line
			SelectedIDE_FinalLine = "";
			SelectedIDE_Assemble_Counter = 0;
			--Count items in IDE table
			SelectedIDE_Final_Count = Table.Count(Vehicle_SelectedLine_IDE_Final);
			while (SelectedIDE_Assemble_Counter &lt; SelectedIDE_Final_Count) do
				SelectedIDE_Assemble_Counter = SelectedIDE_Assemble_Counter + 1;
				--Get Next "Piece" of Ide Data
				SelectedIDE_Assemble_CurrentPiece = Vehicle_SelectedLine_IDE_Final[SelectedIDE_Assemble_Counter];
				if (SelectedIDE_Assemble_Counter == SelectedIDE_Final_Count) then
					--If working with last value, don't add the comma\space\tab
					--Add current piece to the final line
					SelectedIDE_FinalLine = SelectedIDE_FinalLine .. SelectedIDE_Assemble_CurrentPiece;							
				else
					--Add current piece to the final line
					SelectedIDE_FinalLine = SelectedIDE_FinalLine .. SelectedIDE_Assemble_CurrentPiece .. ", \t";
				end
			end
			
			if (Selected_OriginalLineCOL_HasColLineCheck == false) then
				--Assemble Final carcols Line
				SelectedCOL_FinalLine = "";
				SelectedCOL_Assemble_Counter = 0;
				--Count items in IDE table
				SelectedCOL_Final_Count = Table.Count(Vehicle_SelectedLine_COL_Final);
				while (SelectedCOL_Assemble_Counter &lt; SelectedCOL_Final_Count) do
					SelectedCOL_Assemble_Counter = SelectedCOL_Assemble_Counter + 1;
					--Get Next "Piece" of carcols Data
					SelectedCOL_Assemble_CurrentPiece = Vehicle_SelectedLine_COL_Final[SelectedCOL_Assemble_Counter];
					if (SelectedCOL_Assemble_Counter == SelectedCOL_Final_Count) then
						--If working with last value, don't add the comma\space
						--Add current piece to the final line
						SelectedCOL_FinalLine = SelectedCOL_FinalLine .. SelectedCOL_Assemble_CurrentPiece;							
					else
						if (SelectedCOL_Assemble_Counter == 1) then
							--Add current piece to the final line
							SelectedCOL_FinalLine = SelectedCOL_Assemble_CurrentPiece .. ", ";			
						else
							--Add current piece to the final line
							SelectedCOL_FinalLine = SelectedCOL_FinalLine .. SelectedCOL_Assemble_CurrentPiece .. " ";
						end
					end
				end
			end
			
			--Delete existing wft\wtd Files From Temp Folder
			File.Delete(gimi_VehicleTempDir .. "\\*.wft", false, false, false, nil);
			File.Delete(gimi_VehicleTempDir .. "\\*.wtd", false, false, false, nil);
			
			--Copy Chosen wft\wtd Files to Temp Dir
			File.Copy(wftPath, gimi_VehicleTempDir, false, true, false, true, nil);
			Vehicle_wftCopy_ErrorCheck = Application.GetLastError();
			File.Copy(wtdPath, gimi_VehicleTempDir, false, true, false, true, nil);
			Vehicle_wtdCopy_ErrorCheck = Application.GetLastError();
			if ((Vehicle_wftCopy_ErrorCheck ~= 0) or (Vehicle_wtdCopy_ErrorCheck ~= 0)) then
				CancelActions = true;
change_lang_message("page_Vehicle_Selection_texts","56","none","ok")
				Error274 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
				--Log Error
				ErrorLog_Value = "ERROR 274 - "..text_message.."";
				ErrorLogUpdate();
				ExitGIMI();
			else
				--Rename wft\wtd Files to New Replacement Name
				File.Rename(gimi_VehicleTempDir .. "\\" .. wftFile, gimi_VehicleTempDir .. "\\" .. SelectedIDE_Model .. ".wft");
				Vehicle_wftRename_ErrorCheck = Application.GetLastError();
				File.Rename(gimi_VehicleTempDir .. "\\" .. wtdFile, gimi_VehicleTempDir .. "\\" .. SelectedIDE_Model .. ".wtd");
				Vehicle_wtdRename_ErrorCheck = Application.GetLastError();
				if ((Vehicle_wftRename_ErrorCheck ~= 0) or (Vehicle_wtdRename_ErrorCheck ~= 0)) then
					CancelActions = true;
change_lang_message("page_Vehicle_Selection_texts","57","none","ok")
					Error275 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
					--Log Error
					ErrorLog_Value = "ERROR 275 - "..text_message.."";
					ErrorLogUpdate();
					ExitGIMI();
				else
					--Create script to be used to install the vehicle
					---Assign Variables
					Vehicle_InstallScript_Table = {};
					
					--wft file
					Vehicle_InstallScript_Table[1] = "|ImgAdd|" .. SelectedIDE_Model .. ".wft|pc\\models\\cdimages\\vehicles.img|";
					--wtd file
					Vehicle_InstallScript_Table[2] = "|ImgAdd|" .. SelectedIDE_Model .. ".wtd|pc\\models\\cdimages\\vehicles.img|";
					--handling
					Vehicle_InstallScript_Table[3] = "|ReplaceText|" .. SelectedIDE_Handling .. "|common\\data\\handling.dat|" .. SelectedHAN_FinalLine .. "|abort|";
					--ide
					Vehicle_InstallScript_Table[4] = "|ReplaceText|" .. SelectedIDE_Handling .. "," .. "|common\\data\\vehicles.ide|" .. SelectedIDE_FinalLine .. "|abort|";
					--carcols
					if (Selected_OriginalLineCOL_HasColLineCheck == false) then
						Vehicle_InstallScript_Table[5] = "|ReplaceText|" .. SelectedIDE_Model .. "," .. "|common\\data\\carcols.dat|" .. SelectedCOL_FinalLine .. "|abort|";
					end
					
					--Write table to install file
					TextFile.WriteFromTable(Vehicle_InstallScript, Vehicle_InstallScript_Table, false);
					
					
					
					---------Perform Installation-------------------------------------------------------------------
					
					--Assign Necessary Variables
					BackupFiles = true;
					Simulate = false;
					ModInstallType_Text = "Install";
					ActiveScript = Vehicle_InstallScript;
					ModFolder = gimi_VehicleTempDir;
					--Assign Script
					ScriptsFound = {};
					ScriptsFound[1] = ActiveScript;
					ScriptsFoundCurrentNum = 1;
					--Assign Default Mod Info Variables
                    ModName = VehicleName_Text;
                    ModVer = "1.0";
change_lang_message("page_Mod_Info_texts","61","none","none")
                    ModDesc = text_message;
                    ModExtraInfo = text_message;
change_lang_message("page_Mod_Info_texts","62","none","none")
                    ModEmail = text_message;
                    ModURL = text_message;
                    ModCreator = text_message;

					if (CancelActions ~= true) then
						--Perform Actions
						ModInstall();
					end
				end
			end
		end
	end
end

--Enable Install Button
Button.SetEnabled("continue_button", true);
change_lang_message("page_Vehicle_Selection_texts","58","none","none")
Button.SetText("continue_button", text_message);</Script>
