--See if a value exists in a table, and what line #  (function is Case-Sensitive)
function SearchTable (SearchTable_String,SearchTable_Table)
	--Assign Variables
	SearchTable_TableCount = 0;
	SearchTable_Counter = 0;
	SearchTable_ReturnLine = {};
	SearchTable_ReturnPos = {};
	
	--Count table lines
	SearchTable_TableCount = Table.Count(SearchTable_Table);
	while (SearchTable_Counter &lt; SearchTable_TableCount) do
		SearchTable_Counter = SearchTable_Counter + 1;
		--Get next line in table
		SearchTable_TableLine = SearchTable_Table[SearchTable_Counter];
		--Search Line and See if specified value exists
		SearchTable_SearchResult = String.Find(SearchTable_TableLine, SearchTable_String, 1, false);
		if (SearchTable_SearchResult ~= -1) then  --Value IS found
			--Assign Position
			SearchTable_ReturnPos_Count = Table.Count(SearchTable_ReturnPos);
			SearchTable_ReturnPos_InsertPos = SearchTable_ReturnPos_Count + 1;
			SearchTable_ReturnPos[SearchTable_ReturnPos_InsertPos] = SearchTable_Counter;
			--Assign Value
			SearchTable_ReturnLine_Count = Table.Count(SearchTable_ReturnLine);
			SearchTable_ReturnLine_InsertPos = SearchTable_ReturnLine_Count + 1;
			SearchTable_ReturnLine[SearchTable_ReturnLine_InsertPos] = SearchTable_TableLine;
		end
	end
	
	--Return Values
	return SearchTable_ReturnPos, SearchTable_ReturnLine;
   

end


