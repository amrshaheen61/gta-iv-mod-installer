<Event>
<Name>On Preload</Name>
<Args/>
<Script>--Remove Existing Data Fomr Listboxes
ListBox.DeleteItem("wft_file_list", -1);
ListBox.DeleteItem("wtd_file_list", -1);

--Set wft listbox values
Multiwft_Counter = 0;
while (Multiwft_Counter &lt; VehiclewftFiles_Count) do
	Multiwft_Counter = Multiwft_Counter + 1;
	--Get Next wft File
	Multiwft_CurrentPath = VehiclewftFiles[Multiwft_Counter];
	--Remove Mod Path to Have Only "Local Path" and Filename
	Multiwft_CurrentPathTrimmed = RemoveModFolder(Multiwft_CurrentPath);
	if (Multiwft_CurrentPathTrimmed == nil) then
		--Just use the full path
		Multiwft_CurrentPath_Short = Multiwft_CurrentPath;
	else
		--Use the trimmed path (path without full mod folder)
		Multiwft_CurrentPath_Short = Multiwft_CurrentPathTrimmed;
	end
	--Count existing items in wft listbox
	wft_Listbox_Count = ListBox.GetCount("wft_file_list");
	if (wft_Listbox_Count == -1) then
		CancelActions = true;
change_lang_message("page_Multiple_WFT_texts","17","none","ok")
		Error250 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
		--Log Error
		ErrorLog_Value = "ERROR 250 - "..text_message.."";
		ErrorLogUpdate();
		ExitGIMI();
	else
		wft_Listbox_InsertPos = wft_Listbox_Count + 1;
		--Insert current wft data to listbox
		wft_Listbox_PosAdded = ListBox.AddItem("wft_file_list", Multiwft_CurrentPath_Short, Multiwft_CurrentPath);
	end
end

--Set wtd listbox values
Multiwtd_Counter = 0;
while (Multiwtd_Counter &lt; VehiclewtdFiles_Count) do
	Multiwtd_Counter = Multiwtd_Counter + 1;
	--Get Next wtd File
	Multiwtd_CurrentPath = VehiclewtdFiles[Multiwtd_Counter];
	--Remove Mod Path to Have Only "Local Path" and Filename
	Multiwtd_CurrentPathTrimmed = RemoveModFolder(Multiwtd_CurrentPath);
	if (Multiwtd_CurrentPathTrimmed == nil) then
		--Just use the full path
		Multiwtd_CurrentPath_Short = Multiwtd_CurrentPath;
	else
		--Use the trimmed path (path without full mod folder)
		Multiwtd_CurrentPath_Short = Multiwtd_CurrentPathTrimmed;
	end
	--Count existing items in wtd listbox
	wtd_Listbox_Count = ListBox.GetCount("wtd_file_list");
	if (wtd_Listbox_Count == -1) then
		CancelActions = true;
change_lang_message("page_Multiple_WFT_texts","18","none","ok")
		Error251 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
		--Log Error
		ErrorLog_Value = "ERROR 251 - "..text_message.."";
		ErrorLogUpdate();
		ExitGIMI();
	else
		wtd_Listbox_InsertPos = wtd_Listbox_Count + 1;
		--Insert current wtd data to listbox
		wtd_Listbox_PosAdded = ListBox.AddItem("wtd_file_list", Multiwtd_CurrentPath_Short, Multiwtd_CurrentPath);
	end
end



--If only one wft is shown, auto-select it
if (VehiclewftFiles_Count == 1) then
	ListBox.SelectItem("wft_file_list", 1);
end

--If only one wtd is shown, auto-select it
if (VehiclewtdFiles_Count == 1) then
	ListBox.SelectItem("wtd_file_list", 1);
end</Script>
