	TestImgScript1 = TextFile.ReadToString(TestImgScript);
	TestImgScript2= String.Replace(TestImgScript1, "\\", "\\\\", false);
	TextFile.WriteFromString(TestImgScript, TestImgScript2, false);
	
	--ExecuteImgEditor
	File.Run(_SourceFolder.."\\data\\img.exe", "-script \"" .. TestImgScript .. "\"", "", SW_MINIMIZE, true);
