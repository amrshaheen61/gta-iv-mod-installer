<Right>584</Right>
</Coordinates>
<Ratios>
<WidthRatio>-1.000000</WidthRatio>
<HeightRatio>-1.000000</HeightRatio>
</Ratios>
<Event>
<Name>On Click</Name>
<Args/>
<Script>--1) Assign variables
--2) Verify directories
--3) Extract ZIP\RAR File
--4) Search for script(s)

----------------------------------------------------------
--1) Assign variables
----------------------------------------------------------
Error_InvalidScript = false;
ModLocationValid = true;
CancelActions = false;
--Disable Continue Button
Button.SetEnabled("continue_button", false);
change_lang_message("page_install_texts","13","none","none")
Button.SetText("continue_button", text_message);
IsVehicleMod = false;
MultipleScripts = false;
----------------------------------------------------------
--2) Verify directories
----------------------------------------------------------
--Get game dir from inputbox
GameDir = Input.GetText("install-dir_inputbox");
--Remove End Slash (if there is one)
RemoveEndSlash_Value = GameDir;
RemoveEndSlash();
GameDir = RemoveEndSlash_Value;

--Get mod file\folder dir from inputbox
ModLocation = Input.GetText("mod-location-dir_inputbox");
--Remove End Slash (if there is one)
RemoveEndSlash_Value = ModLocation;
RemoveEndSlash();
ModLocation = RemoveEndSlash_Value;

--Determine if the directory specified is correct
VerifyGameDir();

--If game directory IS valid
if (CancelActions ~= true) then
	--Verify mod file or folder exists
	if (InstallSource == "folder") then
		ModLocationValid = Folder.DoesExist(ModLocation);
	end
	if (InstallSource == "file") then
		ModLocationValid = File.DoesExist(ModLocation);
	end
	--If mod file or folder doesn't exist, show message
	if (ModLocationValid ~= true) then
change_lang_message("page_install_texts","6","none","ok")
		result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
		CancelActions = true;
	end
	
	if (CancelActions ~= true) then
		----------------------------------------------------------
		--3) Extract ZIP\RAR File
		----------------------------------------------------------
		
		--If NOT a file, then assign ModFolder...
		if (InstallSource == "folder") then
			ModFolder = ModLocation;
			--Assign Last Used Mod Path
			AssignLastModPath(ModFolder);
		end
		--If it IS a ZIP\RAR file...
		if (InstallSource == "file") then
			--Generate Mod File Temp Path
			ModPathRandomVar = Math.Random(1000000, 9999999);
			ModFolder = String.Concat(ModFolderTemp, ModPathRandomVar);
			--Get File Extension of Selected Mod File
			ModFilenameTable = String.SplitPath(ModLocation);
			--Assign Last Used Mod Path
			AssignLastModPath(ModFilenameTable.Drive .. ModFilenameTable.Folder);
			--Check for error--
			if (ModFilenameTable == "") then
				CancelActions = true;
change_lang_message("page_install_texts","7","none","ok")
				Error150 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
				--Log Error--
				
				ErrorLog_Value = "ERROR 150 - "..text_message.."";
				ErrorLogUpdate();
			else
				ModFileExtension = ModFilenameTable.Extension
			end
			--End Error Check--
			if (CancelActions ~= true) then
				--If File Extension is ".zip"...
				if (ModFileExtension == ".zip") then
					--Create temporary mod folder in temp dir
					Folder.Create(ModFolder);
					--Extract ZIP contents
					Zip.Extract(ModLocation, {"*.*"}, ModFolder, true, true, "", ZIP_OVERWRITE_ALWAYS, nil);
					--Check for error--
					ErrorCode = Application.GetLastError();
					if (ErrorCode ~= 0) then
						CancelActions = true;
change_lang_message("page_install_texts","8","none","ok")
						
						Error151 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
						--Log Error--
						ErrorLog_Value = "ERROR 151 - "..text_message.."";
						ErrorLogUpdate();
					end
					--End error check--
				end
				--If File Extension is ".rar"...
				if (ModFileExtension == ".rar" or ModFileExtension == ".7z" or ModFileExtension == ".cab" or ModFileExtension == ".iso" or ModFileExtension == ".arc") then
					--Create temporary mod folder in temp dir
					Folder.Create(ModFolder);
					--Copy rar.exe to ModFolder and extract RAR file
			    	File.Copy(_SourceFolder.."\\data\\arc.exe", ModFolder, true, true, false, true, nil);
					File.Run(ModFolder .. "\\arc.exe", "x \"" .. ModLocation .. "\"", ModFolder, SW_MINIMIZE, true);
					--Check for error--
					ErrorCode = Application.GetLastError();
					if (ErrorCode ~= 0) then
						CancelActions = true;
change_lang_message("page_install_texts","9","none","ok")
						Error152 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
						--Log Error--
						ErrorLog_Value = "ERROR 152 - "..text_message.."";
						ErrorLogUpdate();
					end				
					--End error check--
					--Delete Rar.exe from temp mod folder
					File.Delete(ModFolder .. "\\arc.exe", false, false, false, nil);
				end
			end
		end

		if (CancelActions ~= true) then
			----------------------------------------------------------
			--4) Search for script(s)
			----------------------------------------------------------
			--See if there's a script(s) file. If so, then use it
			if (ScriptSpecified ~= true) then
				ScriptsFound = File.Find(ModFolder, "*." .. ScriptExt, true, false, nil);
			end
			if (ScriptsFound ~= nil) then
				--Specify type of mod
				IsVehicleMod = false;
				--See if it's a backup script
				Script_BackupScriptInstall_Check_Table = String.SplitPath(ScriptsFound[1]);
				Script_BackupScriptInstall_Check_Filename = Script_BackupScriptInstall_Check_Table.Filename;
				if (Script_BackupScriptInstall_Check_Filename == "backup") then
					CancelActions = true;
change_lang_message("page_install_texts","10","none","ok")
					result = Dialog.Message(text_title,text_message , MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
				else
				
					--See if there's multiple scripts found
					ScriptsFoundCount = Table.Count(ScriptsFound);
					--If more than one script found...
					if (ScriptsFoundCount &gt; 1) then
						MultipleScripts = true;
					else
						MultipleScripts = false;
					end
				end
			else
			
			--++++++++++++++++++++++++++++++++++++++++
				--+See if the mod is a vehicle++--
				---Search wft files for each vehicle type
				VehiclewftSearch_Counter = 0;
				VehiclewftFiles = {};
				VehicleList_Count = Table.Count(VehicleList);
				if (VehicleList_Count == 0) then
change_lang_message("page_install_texts","11","none","ok")
					result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
				else
					while (VehiclewftSearch_Counter &lt; VehicleList_Count) do
						VehiclewftSearch_Counter = VehiclewftSearch_Counter + 1;
						--Get Current Vehicle Name
						VehiclewftSearch_CurrentName = VehicleList[VehiclewftSearch_Counter];
						--See if file with current name exists
						VehiclewftSearch = File.Find(ModFolder, VehiclewftSearch_CurrentName .. ".wft", true, false, nil);
						if (VehiclewftSearch ~= nil) then
							--Count # of Found Files With The Current Name (i.e. multiple "banshee.wft" files)
							VehiclewftSearch_Count = Table.Count(VehiclewftSearch);
							VehiclewftSearch_InsertPos = VehiclewftSearch_Count + 1;
							VehiclewftSearch_Counter2 = 0;
							while (VehiclewftSearch_Counter2 &lt; VehiclewftSearch_Count)do
								VehiclewftSearch_Counter2 = VehiclewftSearch_Counter2 + 1;
								--Get next Alike wft Name
								VehiclewftSearch_CurrentName = VehiclewftSearch[VehiclewftSearch_Counter2];
								--Insert wft Path\Filename into wft Table
								VehiclewftFiles_CountTable = Table.Count(VehiclewftFiles);
								VehiclewftFiles_InsertPos = VehiclewftFiles_CountTable + 1;
								VehiclewftFiles[VehiclewftFiles_InsertPos] = VehiclewftSearch_CurrentName;
							end
						end
					end
				end
				
				---Search wtd files for each vehicle type
				VehiclewtdSearch_Counter = 0;
				VehiclewtdFiles = {};
				VehicleList_Count = Table.Count(VehicleList);
				if (VehicleList_Count == 0) then
change_lang_message("page_install_texts","11","none","ok")
				result = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
					else
					while (VehiclewtdSearch_Counter &lt; VehicleList_Count) do
						VehiclewtdSearch_Counter = VehiclewtdSearch_Counter + 1;
						--Get Current Vehicle Name
						VehiclewtdSearch_CurrentName = VehicleList[VehiclewtdSearch_Counter];
						--See if file with current name exists
						VehiclewtdSearch = File.Find(ModFolder, VehiclewtdSearch_CurrentName .. ".wtd", true, false, nil);
						if (VehiclewtdSearch ~= nil) then
							--Count # of Found Files With The Current Name (i.e. multiple "banshee.wft" files)
							VehiclewtdSearch_Count = Table.Count(VehiclewtdSearch);
							VehiclewtdSearch_InsertPos = VehiclewtdSearch_Count + 1;
							VehiclewtdSearch_Counter2 = 0;
							while (VehiclewtdSearch_Counter2 &lt; VehiclewtdSearch_Count)do
								VehiclewtdSearch_Counter2 = VehiclewtdSearch_Counter2 + 1;
								--Get next Alike wft Name
								VehiclewtdSearch_CurrentName = VehiclewtdSearch[VehiclewtdSearch_Counter2];
								--Insert wft Path\Filename into wft Table
								VehiclewtdFiles_CountTable = Table.Count(VehiclewtdFiles);
								VehiclewtdFiles_InsertPos = VehiclewtdFiles_CountTable + 1;
								VehiclewtdFiles[VehiclewtdFiles_InsertPos] = VehiclewtdSearch_CurrentName;
							end
						end
					end
				end
				
				--Count How Many wft\wtd Files Were Found
				VehiclewftFiles_Count = Table.Count(VehiclewftFiles);
				VehiclewtdFiles_Count = Table.Count(VehiclewtdFiles);
				if ((VehiclewftFiles_Count == 0) or (VehiclewtdFiles_Count == 0)) then
					--If script is NOT found AND mod is NOT a compatible vehicle...
					CancelActions = true;
change_lang_message("page_install_texts","12","none","ok")
					Error153 = Dialog.Message(text_title, text_message, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
					--Log Error--
					ErrorLog_Value = "ERROR 153 - "..text_message.."";
					ErrorLogUpdate();
				else
					IsVehicleMod = true;
				end
			end
	
		
		
			----------------------------------------------------
			--Get Mod Info (name, creator, URL, etc)
			----------------------------------------------------

			if ((CancelActions ~= true) and (IsVehicleMod ~= true)) then
				--Assign variables used in the function
				FunctionType = "ModInfo";
				ScriptsFoundCurrentNum = 1;
				--Call the function
				DisectScript();
			end
		end
	end
end

--Enable Continue Button
Button.SetEnabled("continue_button", true);
change_lang_message("page_install_texts","14","none","none")
Button.SetText("continue_button", text_message);

if (CancelActions ~= true) then --If allowed to go to next page....
	--Go to "multiple scripts" page if necessary
	if (MultipleScripts == true) then
		--Assign first script to be selected
		CurrentScriptSelected = 1;
		Page.Jump("Script Selection");
	else
		--If there's only 1 script, jump to "mod info" page
		ModInstallType_Text = "Install";
		if (IsVehicleMod == true) then
			if ((VehiclewftFiles_Count &gt; 1) or (VehiclewtdFiles_Count &gt; 1)) then
				--If there's more than one wft or wtd file
				Page.Jump("Multiple WFT");
			else
				--If there's only one wft and wtd file
				---Parse wft filepath to get filename only
				wftSplitPathTable = String.SplitPath(VehiclewftFiles[1]);
				wftFile = wftSplitPathTable.Filename .. wftSplitPathTable.Extension;
				wftPath = VehiclewftFiles[1];
				---Parse wtd filepath to get filename only
				wtdSplitPathTable = String.SplitPath(VehiclewtdFiles[1]);
				wtdFile = wtdSplitPathTable.Filename .. wtdSplitPathTable.Extension;
				wtdPath = VehiclewtdFiles[1];
				Page.Jump("Vehicle Info");			
			end
		else
			Page.Jump("Mod Info");
		end
	end
end</Script>
