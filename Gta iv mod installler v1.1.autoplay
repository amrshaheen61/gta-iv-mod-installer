function GetLastModPath ()
	
	--Read last used mod path path from ini file
	LastUsedModPath = INIFile.GetValue(gimiIniFile, "gimi", "LastUsedModPath");
	if (LastUsedModPath == "") then
		--Not Found. Assign desktop as default mod path
		AssignLastModPath(_DesktopFolder);
		return _DesktopFolder;
	else
		--See if path still exists
		LastModPathExists = Folder.DoesExist(LastUsedModPath);
		if (LastModPathExists == true) then
			--DOES exist.
			return LastUsedModPath;
		else
			--Does NOT exist. Assign desktop as default dir
			AssignLastModPath(_DesktopFolder);
			return _DesktopFolder;
		end
	end
end

