function RemoveTabSpace (LineValue)
	NoSpaces = String.Replace(LineValue, " ", "", false);
	NoSpacesTabs = String.Replace(NoSpaces, "\t", "", false);
	return NoSpacesTabs;
end

--+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

function RemoveTabDoubleSpace (LineValueInput)
	--Count Line Length
	LineValueInput_Count = String.Length(LineValueInput);
	--Replace Tabs With Spaces
	NoTabs = String.Replace(LineValueInput, "\t", " ", false);
	--Replace DoubleSpaces With Spaces
	RemoveTabDoubleSpace_Counter = 0;
	while (RemoveTabDoubleSpace_Counter &lt; LineValueInput_Count) do
		RemoveTabDoubleSpace_Counter = RemoveTabDoubleSpace_Counter + 1;
		NoTabsDoubleSpaces = String.Replace(NoTabs, "  ", " ", false);
		NoTabs = NoTabsDoubleSpaces;
	end
	return NoTabsDoubleSpaces;
end

