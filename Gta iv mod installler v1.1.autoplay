function VerifyLineCOL (VerifyLineCOL_Value_Table,VerifyLineCOL_HandlingName_String)
	--Assign Variables
	VerifyLineCOL_Return = {}; --Returns a table containing all verified lines
	VerifyLineCOL_Counter = 0;
	
	--Count How Many COL Lines Were Specified
	VerifyLineCOL_Value_Table_Count = Table.Count(VerifyLineCOL_Value_Table);
	while (VerifyLineCOL_Counter &lt; VerifyLineCOL_Value_Table_Count) do
		VerifyLineCOL_NumCheck_Stop = false;
		VerifyLineCOL_Counter = VerifyLineCOL_Counter + 1;
		--Get Next Line That Is to Be Verified
		VerifyLineCOL_CurrentLineFull = VerifyLineCOL_Value_Table[VerifyLineCOL_Counter];

		-------------------------Run All Verification Proceedures-----------------------------
		
		--See Where the Model Name Is In Current Line
		VerifyLineCOL_NamePos = String.Find(VerifyLineCOL_CurrentLineFull, VehicleIDE_Model, 1, true);
		VerifyLineCOL_CurrentLineFull_Length = String.Length(VerifyLineCOL_CurrentLineFull);

		if (VerifyLineCOL_NamePos == -1) then
			VerifyLineCOL_NumCheck_Stop = true;
		else
			--Get New Line and Remove Info to Left of Model Name Number
			VerifyLineCOL_NameCharsToGet = VerifyLineCOL_CurrentLineFull_Length - VerifyLineCOL_NamePos + 1;
			VerifyLineCOL_CurrentLine_NewOld = String.Right(VerifyLineCOL_CurrentLineFull, VerifyLineCOL_NameCharsToGet);
			--Remove Tabs\DoubleSpaces (if there are any)
			VerifyLineCOL_CurrentLine_New = RemoveTabDoubleSpace(VerifyLineCOL_CurrentLine_NewOld);
			--Delimit Line
			VerifyLineCOL_CurrentLine_Table = Delimit(VerifyLineCOL_CurrentLine_New, " ", false);
			--Make Sure Model Name Has No Extra Characters
			VerifyLineCOL_ExtraCharCheck = VerifyLineCOL_CurrentLine_Table[1];
			VerifyLineCOL_ModelComma = VehicleIDE_Model .. ",";
			if (VerifyLineCOL_ExtraCharCheck ~= VerifyLineCOL_ModelComma) then
				--Model Name Has Extra Characters
				VerifyLineCOL_NumCheck_Stop = true;
			else
				--Count Fields In Current Line
				VerifyLineCOL_CurrentLine_Table_Count = Table.Count(VerifyLineCOL_CurrentLine_Table);
				if (VerifyLineCOL_CurrentLine_Table_Count &lt; 2) then
					--The Line Is Invalid (Not Enough Fields, Needs at least two)
					VerifyLineCOL_NumCheck_Stop = true;
				else
					--Make sure number values have two nums separated by comma (i.e. "63,1")
					VerifyLineCOL_NumCheck_Counter = 0;
					while ((VerifyLineCOL_NumCheck_Counter &lt; VerifyLineCOL_CurrentLine_Table_Count) and (VerifyLineCOL_NumCheck_Stop ~= true)) do
						VerifyLineCOL_NumCheck_Counter = VerifyLineCOL_NumCheck_Counter + 1;
						--Get Next Piece of Carcols Line
						VerifyLineCOL_NumCheck_CurrentLine = VerifyLineCOL_CurrentLine_Table[VerifyLineCOL_NumCheck_Counter];
						if (VerifyLineCOL_NumCheck_Counter == 1) then
							--It's the model name so don't check the numerical value
						else
							--Make sure there are two numbers, separated by a comma
							VerifyLineCOL_NumCheck_Delimit = Delimit(VerifyLineCOL_NumCheck_CurrentLine, ",", true);
							--Make sure only  numbers were returned
							VerifyLineCOL_NumCheck_Delimit_Count = Table.Count(VerifyLineCOL_NumCheck_Delimit);														
							if (VerifyLineCOL_NumCheck_Delimit_Count ~=car4) then
                          	VerifyLineCOL_NumCheck_Stop = true;
							else
																
					          --Make sure both pieces are numerical values
								VerifyLineCOL_NumCheck_IsNumeric1 = IsNumeric(VerifyLineCOL_NumCheck_Delimit[1]);
								VerifyLineCOL_NumCheck_IsNumeric2 = IsNumeric(VerifyLineCOL_NumCheck_Delimit[2]);
								VerifyLineCOL_NumCheck_IsNumeric3 = IsNumeric(VerifyLineCOL_NumCheck_Delimit[3]);
								if ((VerifyLineCOL_NumCheck_IsNumeric1 == false) or (VerifyLineCOL_NumCheck_IsNumeric2 == false)or (VerifyLineCOL_NumCheck_IsNumeric3 == false)) then
									--At least one of the numbers are not numeric
									VerifyLineCOL_NumCheck_Stop = true;
									
								
								end
							end
							
						end
					end
				end
			end
		end
		if (VerifyLineCOL_NumCheck_Stop ~= true) then
			--Assemble Final carcols Line
			VerifyLineCOL_FinalLine = "";
			VerifyLineCOL_Assemble_Counter = 0;
			while (VerifyLineCOL_Assemble_Counter &lt; VerifyLineCOL_CurrentLine_Table_Count) do
				VerifyLineCOL_Assemble_Counter = VerifyLineCOL_Assemble_Counter + 1;
				--Get Next "Piece" of carcols Data
				VerifyLineCOL_Assemble_CurrentPiece = VerifyLineCOL_CurrentLine_Table[VerifyLineCOL_Assemble_Counter];
				if (VerifyLineCOL_Assemble_Counter == VerifyLineCOL_CurrentLine_Table_Count) then
					--If working with last value, don't add the comma\space
					--Add current piece to the final line
					VerifyLineCOL_FinalLine = VerifyLineCOL_FinalLine .. VerifyLineCOL_Assemble_CurrentPiece;							
				else
					--Add current piece to the final line
					VerifyLineCOL_FinalLine = VerifyLineCOL_FinalLine .. VerifyLineCOL_Assemble_CurrentPiece .. " ";
				end
			end
			if (VerifyLineCOL_FinalLine ~= "") then
				--Add Current Line to Return Table
				VerifyLineCOL_Return_Count = Table.Count(VerifyLineCOL_Return);
				VerifyLineCOL_Return_InsertPos = VerifyLineCOL_Return_Count + 1;
				VerifyLineCOL_Return[VerifyLineCOL_Return_InsertPos] = VerifyLineCOL_FinalLine;
			end
		end
	end
	--Return Table Containing All Valid Carcols Lines Only
	return VerifyLineCOL_Return;
end


